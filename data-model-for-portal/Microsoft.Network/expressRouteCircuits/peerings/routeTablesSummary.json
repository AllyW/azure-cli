{
    "resourceType": "expressRouteCircuits/peerings/routeTablesSummary",
    "apiVersion": "2023-09-01",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/network/express-route"
    },
    "commands": [
        {
            "name": "az network express-route list-route-tables-summary",
            "description": "Show the current routing table summary of an ExpressRoute circuit peering.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/expressRouteCircuits/{circuitName}/peerings/{peeringName}/routeTablesSummary/{devicePath}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/network/express-route#az-network-express-route-list-route-tables-summary"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--path]",
                            "[--peering-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List Route Table Summary",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.circuitName]"
                        },
                        {
                            "name": "--path",
                            "value": "[path.devicePath]"
                        },
                        {
                            "name": "--peering-name",
                            "value": "[path.peeringName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}