{
    "resourceType": "networkWatchers/flowLogs",
    "apiVersion": "2022-01-01",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/network/watcher/flow-log"
    },
    "commands": [
        {
            "name": "az network watcher flow-log delete",
            "description": "Delete the specified flow log resource.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/networkWatchers/{networkWatcherName}/flowLogs/{flowLogName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/network/watcher/flow-log#az-network-watcher-flow-log-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--network-watcher-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete the specified flow log resource.",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.flowLogName]"
                        },
                        {
                            "name": "--network-watcher-name",
                            "value": "[path.networkWatcherName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az network watcher flow-log show",
            "description": "Get the flow log configuration of a network security group.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/networkWatchers/{networkWatcherName}/flowLogs/{flowLogName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/network/watcher/flow-log#az-network-watcher-flow-log-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--network-watcher-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Get the flow log configuration of a network security group.",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.flowLogName]"
                        },
                        {
                            "name": "--network-watcher-name",
                            "value": "[path.networkWatcherName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}