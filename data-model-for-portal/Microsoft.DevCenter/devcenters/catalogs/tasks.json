            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/devcenter/admin/catalog-task#az-devcenter-admin-catalog-task-get-error-detail"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--catalog-name]",
                            "[--dev-center]",
                            "[--dev-center-name]",
                            "[--resource-group]",
                            "[--subscription]",
                            "[--name]",
                            "[--task-name]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Get error detail",
                    "parameters": [
                        {
                            "name": "--catalog-name",
                            "value": "[path.catalogName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.taskName]"
                        },
                        {
                            "name": "--dev-center",
                            "value": "[path.devCenterName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
