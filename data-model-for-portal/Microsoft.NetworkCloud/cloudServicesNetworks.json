                    {
                        "parameters": [
                            "[--name]",
                            "[--cloud-services-network-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Get cloud services network",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.cloudServicesNetworkName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
