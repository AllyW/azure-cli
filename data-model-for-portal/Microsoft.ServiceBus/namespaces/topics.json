{
    "resourceType": "namespaces/topics",
    "apiVersion": "2022-01-01-preview",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/servicebus/topic"
    },
    "commands": [
        {
            "name": "az servicebus topic delete",
            "description": "Delete a topic from the specified namespace and resource group.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ServiceBus/namespaces/{namespaceName}/topics/{topicName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/servicebus/topic#az-servicebus-topic-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--namespace-name]",
                            "[--resource-group]",
                            "[--subscription]",
                            "[--name]",
                            "[--topic-name]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete a topic from the specified namespace and resource group.",
                    "parameters": [
                        {
                            "name": "--namespace-name",
                            "value": "[path.namespaceName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.topicName]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az servicebus topic show",
            "description": "Get a description for the specified topic.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ServiceBus/namespaces/{namespaceName}/topics/{topicName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/servicebus/topic#az-servicebus-topic-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--namespace-name]",
                            "[--resource-group]",
                            "[--subscription]",
                            "[--name]",
                            "[--topic-name]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Get a description for the specified topic.",
                    "parameters": [
                        {
                            "name": "--namespace-name",
                            "value": "[path.namespaceName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.topicName]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az servicebus topic authorization-rule list",
            "description": "List authorization rules for a topic.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ServiceBus/namespaces/{namespaceName}/topics/{topicName}/authorizationRules",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/servicebus/topic/authorization-rule#az-servicebus-topic-authorization-rule-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--namespace-name]",
                            "[--resource-group]",
                            "[--subscription]",
                            "[--topic-name]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "shows list of Authorization Rule by Service Bus Topic",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--namespace-name",
                            "value": "[path.namespaceName]"
                        },
                        {
                            "name": "--topic-name",
                            "value": "[path.topicName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az servicebus topic subscription list",
            "description": "List all the subscriptions under a specified topic.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ServiceBus/namespaces/{namespaceName}/topics/{topicName}/subscriptions",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/servicebus/topic/subscription#az-servicebus-topic-subscription-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--namespace-name]",
                            "[--resource-group]",
                            "[--subscription]",
                            "[--topic-name]",
                            "[--skip]",
                            "[--top]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List all the subscriptions under a specified topic.",
                    "parameters": [
                        {
                            "name": "--namespace-name",
                            "value": "[path.namespaceName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        },
                        {
                            "name": "--topic-name",
                            "value": "[path.topicName]"
                        }
                    ]
                }
            ]
        }
    ]
}