{
    "resourceType": "bareMetalMachines",
    "apiVersion": "2023-10-01-preview",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/networkcloud/baremetalmachine"
    },
    "commands": [
        {
            "name": "az networkcloud baremetalmachine list",
            "description": "List bare metal machines in the provided resource group or subscription.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.NetworkCloud/bareMetalMachines",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/networkcloud/baremetalmachine#az-networkcloud-baremetalmachine-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List bare metal machines for resource group",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az networkcloud baremetalmachine reimage",
            "description": "Reimage the provided bare metal machine.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.NetworkCloud/bareMetalMachines/{bareMetalMachineName}/reimage",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/networkcloud/baremetalmachine#az-networkcloud-baremetalmachine-reimage"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--bare-metal-machine-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Reimage bare metal machine",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.bareMetalMachineName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az networkcloud baremetalmachine restart",
            "description": "Restart the provided bare metal machine.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.NetworkCloud/bareMetalMachines/{bareMetalMachineName}/restart",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/networkcloud/baremetalmachine#az-networkcloud-baremetalmachine-restart"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--bare-metal-machine-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Restart bare metal machine",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.bareMetalMachineName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az networkcloud baremetalmachine show",
            "description": "Get properties of the provided bare metal machine.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.NetworkCloud/bareMetalMachines/{bareMetalMachineName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/networkcloud/baremetalmachine#az-networkcloud-baremetalmachine-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--bare-metal-machine-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Get bare metal machine",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.bareMetalMachineName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az networkcloud baremetalmachine start",
            "description": "Start the provided bare metal machine.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.NetworkCloud/bareMetalMachines/{bareMetalMachineName}/start",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/networkcloud/baremetalmachine#az-networkcloud-baremetalmachine-start"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--bare-metal-machine-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Start bare metal machine",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.bareMetalMachineName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az networkcloud baremetalmachine uncordon",
            "description": "Uncordon the provided bare metal machine's Kubernetes node.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.NetworkCloud/bareMetalMachines/{bareMetalMachineName}/uncordon",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/networkcloud/baremetalmachine#az-networkcloud-baremetalmachine-uncordon"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--bare-metal-machine-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Uncordon bare metal machine",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.bareMetalMachineName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}