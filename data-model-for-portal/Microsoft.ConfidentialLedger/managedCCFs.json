{
    "resourceType": "managedCCFs",
    "apiVersion": "2023-01-26-preview",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/confidentialledger/managedccfs"
    },
    "commands": [
        {
            "name": "az confidentialledger managedccfs delete",
            "description": "Delete a Managed CCF instance.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ConfidentialLedger/managedCCFs/{appName}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/confidentialledger/managedccfs#az-confidentialledger-managedccfs-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete a Managed CCF instance",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.appName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az confidentialledger managedccfs list",
            "description": "View the Azure Managed CCF instances in a resource group.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ConfidentialLedger/managedCCFs",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/confidentialledger/managedccfs#az-confidentialledger-managedccfs-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "View the Managed CCF instances",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az confidentialledger managedccfs show",
            "description": "View the details of an instance.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ConfidentialLedger/managedCCFs/{appName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/confidentialledger/managedccfs#az-confidentialledger-managedccfs-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "View the details of a Managed CCF instance",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.appName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}