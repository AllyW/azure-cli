{
    "resourceType": "dataCollectionEndpoints",
    "apiVersion": "2023-03-11",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/monitor/data-collection/endpoint"
    },
    "commands": [
        {
            "name": "az monitor data-collection endpoint delete",
            "description": "Delete a data collection endpoint.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Insights/dataCollectionEndpoints/{dataCollectionEndpointName}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/monitor/data-collection/endpoint#az-monitor-data-collection-endpoint-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--data-collection-endpoint-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete data collection endpoint",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.dataCollectionEndpointName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az monitor data-collection endpoint list",
            "description": "List all data collection endpoints in the specified subscription",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Insights/dataCollectionEndpoints",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/monitor/data-collection/endpoint#az-monitor-data-collection-endpoint-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List data collection endpoints by resource group",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az monitor data-collection endpoint show",
            "description": "Get the specified data collection endpoint.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Insights/dataCollectionEndpoints/{dataCollectionEndpointName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/monitor/data-collection/endpoint#az-monitor-data-collection-endpoint-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--data-collection-endpoint-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Get data collection endpoint",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.dataCollectionEndpointName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az monitor data-collection endpoint association list",
            "description": "List associations for the specified data collection endpoint.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Insights/dataCollectionEndpoints/{dataCollectionEndpointName}/associations",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/monitor/data-collection/endpoint/association#az-monitor-data-collection-endpoint-association-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--data-collection-endpoint-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List associations for the specified data collection endpoint.",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.dataCollectionEndpointName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}