{
    "resourceType": "selfHelp",
    "apiVersion": "2024-03-01-preview",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/self-help/solution-self-help"
    },
    "commands": [
        {
            "name": "az self-help solution-self-help show",
            "description": "Get Self Help Solutions for a given solutionId. Self Help Solutions consist of rich instructional video tutorials, links and guides to public documentation related to a specific problem that enables users to troubleshoot Azure issues.",
            "path": "/providers/Microsoft.Help/selfHelp/{solutionId}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/self-help/solution-self-help#az-self-help-solution-self-help-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--solution-id]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Get self help solution result",
                    "parameters": [
                        {
                            "name": "--solution-id",
                            "value": "[path.solutionId]"
                        }
                    ]
                }
            ]
        }
    ]
}