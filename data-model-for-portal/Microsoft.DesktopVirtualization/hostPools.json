{
    "resourceType": "hostPools",
    "apiVersion": "2021-07-12",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/desktopvirtualization/hostpool"
    },
    "commands": [
        {
            "name": "az desktopvirtualization hostpool delete",
            "description": "Delete a host pool.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.DesktopVirtualization/hostPools/{hostPoolName}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/desktopvirtualization/hostpool#az-desktopvirtualization-hostpool-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--resource-group]",
                            "[--subscription]",
                            "[--force]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete host pool",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.hostPoolName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
