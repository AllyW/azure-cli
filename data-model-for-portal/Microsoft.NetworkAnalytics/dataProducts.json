                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--data-product-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Get DP",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.dataProductName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}