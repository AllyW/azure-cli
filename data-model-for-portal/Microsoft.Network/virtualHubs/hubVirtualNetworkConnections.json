            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/network/vhub/connection#az-network-vhub-connection-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--resource-group]",
                            "[--subscription]",
                            "[--vhub-name]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete a virtual hub VNet connection.",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.connectionName]"
                        },
                        {
                            "name": "--vhub-name",
