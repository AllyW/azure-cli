{
    "resourceType": "dnsForwardingRulesets",
    "apiVersion": "2022-07-01",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/dns-resolver/forwarding-rule"
    },
    "commands": [
        {
            "name": "az dns-resolver forwarding-rule list",
            "description": "Lists forwarding rules in a DNS forwarding ruleset.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/dnsForwardingRulesets/{dnsForwardingRulesetName}/forwardingRules",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/dns-resolver/forwarding-rule#az-dns-resolver-forwarding-rule-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--ruleset-name]",
                            "[--resource-group]",
                            "[--subscription]",
                            "[--top]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Lists forwarding rules in a DNS forwarding ruleset.",
                    "parameters": [
                        {
                            "name": "--ruleset-name",
                            "value": "[path.dnsForwardingRulesetName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az dns-resolver forwarding-ruleset delete",
            "description": "Deletes a DNS forwarding ruleset. WARNING: This operation cannot be undone. All forwarding rules within the ruleset will be deleted.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/dnsForwardingRulesets/{dnsForwardingRulesetName}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/dns-resolver/forwarding-ruleset#az-dns-resolver-forwarding-ruleset-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--if-match]",
                            "[--name]",
                            "[--dns-forwarding-ruleset-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete DNS forwarding ruleset",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.dnsForwardingRulesetName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az dns-resolver forwarding-ruleset list",
            "description": "Lists DNS forwarding rulesets in all resource groups of a subscription.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/dnsForwardingRulesets",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/dns-resolver/forwarding-ruleset#az-dns-resolver-forwarding-ruleset-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-group]",
                            "[--subscription]",
                            "[--top]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List DNS forwarding rulesets by resource group",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az dns-resolver forwarding-ruleset show",
            "description": "Gets a DNS forwarding ruleset properties.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/dnsForwardingRulesets/{dnsForwardingRulesetName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/dns-resolver/forwarding-ruleset#az-dns-resolver-forwarding-ruleset-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--dns-forwarding-ruleset-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Retrieve DNS forwarding ruleset",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.dnsForwardingRulesetName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az dns-resolver vnet-link list",
            "description": "Lists virtual network links to a DNS forwarding ruleset.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/dnsForwardingRulesets/{dnsForwardingRulesetName}/virtualNetworkLinks",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/dns-resolver/vnet-link#az-dns-resolver-vnet-link-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--ruleset-name]",
                            "[--resource-group]",
                            "[--subscription]",
                            "[--top]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List virtual network links to a DNS forwarding ruleset",
                    "parameters": [
                        {
                            "name": "--ruleset-name",
                            "value": "[path.dnsForwardingRulesetName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}