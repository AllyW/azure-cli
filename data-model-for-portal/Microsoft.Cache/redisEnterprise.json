{
    "resourceType": "redisEnterprise",
    "apiVersion": "2023-03-01-preview",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/redisenterprise"
    },
    "commands": [
        {
            "name": "az redisenterprise delete",
            "description": "Delete a RedisEnterprise cache cluster.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Cache/redisEnterprise/{clusterName}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/redisenterprise#az-redisenterprise-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--cluster-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete a RedisEnterprise cache cluster.",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.clusterName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az redisenterprise list",
            "description": "List all RedisEnterprise clusters in the specified subscription.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Cache/redisEnterprise",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/redisenterprise#az-redisenterprise-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List all RedisEnterprise clusters in the specified subscription.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az redisenterprise show",
            "description": "Get information about a RedisEnterprise cluster",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Cache/redisEnterprise/{clusterName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/redisenterprise#az-redisenterprise-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--cluster-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Get information about a RedisEnterprise cluster",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.clusterName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az redisenterprise database list",
            "description": "List all databases in the specified RedisEnterprise cluster.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Cache/redisEnterprise/{clusterName}/databases",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/redisenterprise/database#az-redisenterprise-database-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--cluster-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List all databases in the specified RedisEnterprise cluster.",
                    "parameters": [
                        {
                            "name": "--cluster-name",
                            "value": "[path.clusterName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}