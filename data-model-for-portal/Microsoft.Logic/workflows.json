{
    "resourceType": "workflows",
    "apiVersion": "2019-05-01",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/logic/workflow"
    },
    "commands": [
        {
            "name": "az logic workflow delete",
            "description": "Delete a workflow.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Logic/workflows/{workflowName}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/logic/workflow#az-logic-workflow-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-group]",
                            "[--subscription]",
                            "[--name]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete workflow",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.workflowName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az logic workflow list",
            "description": "List workflows",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Logic/workflows",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/logic/workflow#az-logic-workflow-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-group]",
                            "[--subscription]",
                            "[--filter]",
                            "[--top]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List all workflows in a resource group",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az logic workflow show",
            "description": "Show a workflow.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Logic/workflows/{workflowName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/logic/workflow#az-logic-workflow-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-group]",
                            "[--subscription]",
                            "[--name]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Show workflow",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.workflowName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}