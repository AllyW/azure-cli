{
    "resourceType": "vaults/replicationAlertSettings",
    "apiVersion": "2022-08-01",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/site-recovery/alert-setting"
    },
    "commands": [
        {
            "name": "az site-recovery alert-setting show",
            "description": "Get the details of the specified email notification(alert) configuration.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.RecoveryServices/vaults/{resourceName}/replicationAlertSettings/{alertSettingName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/site-recovery/alert-setting#az-site-recovery-alert-setting-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--alert-setting-name]",
                            "[--resource-group]",
                            "[--vault-name]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "alert-setting show",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.alertSettingName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--vault-name",
                            "value": "[path.resourceName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}