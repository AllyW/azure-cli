{
    "resourceType": "simGroups/sims",
    "apiVersion": "2023-09-01",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/mobile-network/sim"
    },
    "commands": [
        {
            "name": "az mobile-network sim delete",
            "description": "Delete the specified SIM.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.MobileNetwork/simGroups/{simGroupName}/sims/{simName}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/mobile-network/sim#az-mobile-network-sim-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-group]",
                            "[--sim-group-name]",
                            "[--name]",
                            "[--sim-name]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete the specified SIM.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--sim-group-name",
                            "value": "[path.simGroupName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.simName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az mobile-network sim show",
            "description": "Get information about the specified SIM.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.MobileNetwork/simGroups/{simGroupName}/sims/{simName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/mobile-network/sim#az-mobile-network-sim-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-group]",
                            "[--sim-group-name]",
                            "[--name]",
                            "[--sim-name]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Show sim",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.simName]"
                        },
                        {
                            "name": "--sim-group-name",
                            "value": "[path.simGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}