                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--catalog-name]",
                            "[--dev-center]",
                            "[--dev-center-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Connects a catalog to enable syncing.",
                    "parameters": [
                        {
                            "name": "--catalog-name",
                            "value": "[path.catalogName]"
                        },
                        {
                            "name": "--dev-center",
                            "value": "[path.devCenterName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
