{
    "resourceType": "nginxDeployments/configurations",
    "apiVersion": "2024-01-01-preview",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/nginx/deployment/configuration"
    },
    "commands": [
        {
            "name": "az nginx deployment configuration delete",
            "description": "Delete an Nginx configuration",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Nginx.NginxPlus/nginxDeployments/{deploymentName}/configurations/{configurationName}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/nginx/deployment/configuration#az-nginx-deployment-configuration-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--configuration-name]",
                            "[--deployment-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Configuration Delete",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.configurationName]"
                        },
                        {
                            "name": "--deployment-name",
                            "value": "[path.deploymentName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az nginx deployment configuration show",
            "description": "Get the properties of a specific NGINX configuration",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Nginx.NginxPlus/nginxDeployments/{deploymentName}/configurations/{configurationName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/nginx/deployment/configuration#az-nginx-deployment-configuration-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--configuration-name]",
                            "[--deployment-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Configuration Get",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.configurationName]"
                        },
                        {
                            "name": "--deployment-name",
                            "value": "[path.deploymentName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}