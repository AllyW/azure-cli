{
    "resourceType": "attestationProviders",
    "apiVersion": "2020-10-01",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/attestation"
    },
    "commands": [
        {
            "name": "az attestation delete",
            "description": "Delete Attestation Service.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Attestation/attestationProviders/{providerName}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/attestation#az-attestation-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--provider-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete Attestation Service.",
                    "parameters": [
                        {
                            "name": "--provider-name",
                            "value": "[path.providerName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az attestation list",
            "description": "Returns a list of attestation providers in a subscription.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Attestation/attestationProviders",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/attestation#az-attestation-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "AttestationProviders_ListByResourceGroup",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az attestation show",
            "description": "Get the status of Attestation Provider.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Attestation/attestationProviders/{providerName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/attestation#az-attestation-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--provider-name]",
