                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--group-name]",
                            "[--resource-group]",
                            "[--name]",
                            "[--security-connector-name]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Gets nested subgroups of given GitLab Group which are onboarded to the connector.",
                    "parameters": [
                        {
                            "name": "--group-name",
