{
    "resourceType": "dataBoxEdgeDevices/storageAccountCredentials",
    "apiVersion": "2023-07-01",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/databoxedge/device/storage-account-credential"
    },
    "commands": [
        {
            "name": "az databoxedge device storage-account-credential delete",
            "description": "Delete the storage account credential.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.DataBoxEdge/dataBoxEdgeDevices/{deviceName}/storageAccountCredentials/{name}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/databoxedge/device/storage-account-credential#az-databoxedge-device-storage-account-credential-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--device-name]",
                            "[--name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete storage account credential",
                    "parameters": [
                        {
                            "name": "--device-name",
                            "value": "[path.deviceName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.name]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az databoxedge device storage-account-credential show",
            "description": "Get the properties of the specified storage account credential.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.DataBoxEdge/dataBoxEdgeDevices/{deviceName}/storageAccountCredentials/{name}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/databoxedge/device/storage-account-credential#az-databoxedge-device-storage-account-credential-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--device-name]",
                            "[--name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Show storage account credential",
                    "parameters": [
                        {
                            "name": "--device-name",
                            "value": "[path.deviceName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.name]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}