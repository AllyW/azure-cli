{
    "resourceType": "clusters",
    "apiVersion": "2023-08-01",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/stack-hci/arc-setting"
    },
    "commands": [
        {
            "name": "az stack-hci arc-setting list",
            "description": "List arc setting resources of HCI cluster.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.AzureStackHCI/clusters/{clusterName}/arcSettings",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/stack-hci/arc-setting#az-stack-hci-arc-setting-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--cluster-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List arc setting resources by HCI cluster",
                    "parameters": [
                        {
                            "name": "--cluster-name",
                            "value": "[path.clusterName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az stack-hci cluster create-identity",
