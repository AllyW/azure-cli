{
    "resourceType": "availabilitySets",
    "apiVersion": "2022-11-01",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/vm/availability-set"
    },
    "commands": [
        {
            "name": "az vm availability-set delete",
            "description": "Delete an availability set.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Compute/availabilitySets/{availabilitySetName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/vm/availability-set#az-vm-availability-set-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--availability-set-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete an availability set.",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.availabilitySetName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az vm availability-set list-sizes",
            "description": "List all available virtual machine sizes that can be used to create a new virtual machine in an existing availability set.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Compute/availabilitySets/{availabilitySetName}/vmSizes",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/vm/availability-set#az-vm-availability-set-list-sizes"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--availability-set-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List VM sizes for an availability set.",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.availabilitySetName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az vm availability-set show",
            "description": "Get information about an availability set.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Compute/availabilitySets/{availabilitySetName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/vm/availability-set#az-vm-availability-set-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--availability-set-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Get information about an availability set.",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.availabilitySetName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}