                    "url": "https://docs.microsoft.com/cli/azure/dataprotection/recovery-point#az-dataprotection-recovery-point-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-group]",
                            "[--subscription]",
                            "[--vault-name]",
                            "[--backup-instance-name]",
                            "[--recovery-point-id]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Get a Recovery Point using recoveryPointId for a Datasource.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        },
                        {
                            "name": "--vault-name",
                            "value": "[path.vaultName]"
                        },
                        {
                            "name": "--backup-instance-name",
                            "value": "[path.backupInstanceName]"
                        },
                        {
