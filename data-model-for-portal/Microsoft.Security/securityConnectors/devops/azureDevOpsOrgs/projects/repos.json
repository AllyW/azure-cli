                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--org-name]",
                            "[--project-name]",
                            "[--repo-name]",
                            "[--resource-group]",
                            "[--name]",
                            "[--security-connector-name]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Get a monitored Azure DevOps repository resource.",
                    "parameters": [
                        {
                            "name": "--org-name",
                            "value": "[path.orgName]"
                        },
                        {
                            "name": "--project-name",
                            "value": "[path.projectName]"
                        },
                        {
                            "name": "--repo-name",
                            "value": "[path.repoName]"
                        },
                        {
