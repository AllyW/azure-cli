{
    "resourceType": "fileWorkspaces",
    "apiVersion": "2024-04-01",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/support/in-subscription/file"
    },
    "commands": [
        {
            "name": "az support in-subscription file list",
            "description": "List all the Files information under a workspace for an Azure subscription.",
            "path": "/subscriptions/{subscriptionId}/providers/Microsoft.Support/fileWorkspaces/{fileWorkspaceName}/files",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/support/in-subscription/file#az-support-in-subscription-file-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--file-workspace-name]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List all the files within a file-workspace for a specified subscription (different from the account subscription)",
                    "parameters": [
                        {
                            "name": "--file-workspace-name",
                            "value": "[path.fileWorkspaceName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az support in-subscription file-workspace create",
            "description": "Create a new file workspace for the specified subscription.",
            "path": "/subscriptions/{subscriptionId}/providers/Microsoft.Support/fileWorkspaces/{fileWorkspaceName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/support/in-subscription/file-workspace#az-support-in-subscription-file-workspace-create"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--file-workspace-name]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Create a file workspace for a specified subscription (different from the account subscription)",
                    "parameters": [
                        {
                            "name": "--file-workspace-name",
                            "value": "[path.fileWorkspaceName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az support in-subscription file-workspace show",
            "description": "Get details for a specific file workspace in an Azure subscription.",
            "path": "/subscriptions/{subscriptionId}/providers/Microsoft.Support/fileWorkspaces/{fileWorkspaceName}",
            "confirmation": false,
            "help": {
                "learnMore": {
