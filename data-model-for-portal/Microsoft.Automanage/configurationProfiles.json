{
    "resourceType": "configurationProfiles",
    "apiVersion": "2022-05-04",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/automanage/configuration-profile"
    },
    "commands": [
        {
            "name": "az automanage configuration-profile delete",
            "description": "Delete a configuration profile",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Automanage/configurationProfiles/{configurationProfileName}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/automanage/configuration-profile#az-automanage-configuration-profile-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--configuration-profile-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "delete configuration-profile",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.configurationProfileName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az automanage configuration-profile list",
            "description": "List configuration profiles within a subscription",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Automanage/configurationProfiles",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/automanage/configuration-profile#az-automanage-configuration-profile-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "list configuration-profile",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az automanage configuration-profile show",
            "description": "Get information about a configuration profile",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Automanage/configurationProfiles/{configurationProfileName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/automanage/configuration-profile#az-automanage-configuration-profile-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--configuration-profile-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "show configuration-profile",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.configurationProfileName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az automanage configuration-profile version list",
            "description": "List configuration profile versions for a configuration profile",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Automanage/configurationProfiles/{configurationProfileName}/versions",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/automanage/configuration-profile/version#az-automanage-configuration-profile-version-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--profile-name]",
                            "[--configuration-profile-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "list configuration-profile version",
                    "parameters": [
                        {
                            "name": "--profile-name",
                            "value": "[path.configurationProfileName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}