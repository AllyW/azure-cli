{
    "resourceType": "clusters/metricsConfigurations",
    "apiVersion": "2023-10-01-preview",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/networkcloud/cluster/metricsconfiguration"
    },
    "commands": [
        {
            "name": "az networkcloud cluster metricsconfiguration delete",
            "description": "Delete the metrics configuration of the provided cluster.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.NetworkCloud/clusters/{clusterName}/metricsConfigurations/{metricsConfigurationName}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/networkcloud/cluster/metricsconfiguration#az-networkcloud-cluster-metricsconfiguration-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--cluster-name]",
                            "[--name]",
                            "[--metrics-configuration-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete the metrics configuration of the provided cluster.",
                    "parameters": [
                        {
                            "name": "--cluster-name",
                            "value": "[path.clusterName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.metricsConfigurationName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az networkcloud cluster metricsconfiguration show",
            "description": "Get metrics configuration of the provided cluster.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.NetworkCloud/clusters/{clusterName}/metricsConfigurations/{metricsConfigurationName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/networkcloud/cluster/metricsconfiguration#az-networkcloud-cluster-metricsconfiguration-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--cluster-name]",
                            "[--name]",
                            "[--metrics-configuration-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Get metrics configuration of the provided cluster.",
                    "parameters": [
                        {
                            "name": "--cluster-name",
                            "value": "[path.clusterName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.metricsConfigurationName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}