                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az network dns zone show",
            "description": "Get a DNS zone. Retrieves the zone properties, but not the record sets within the zone.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/dnsZones/{zoneName}",
            "confirmation": false,
            "help": {
                "learnMore": {
