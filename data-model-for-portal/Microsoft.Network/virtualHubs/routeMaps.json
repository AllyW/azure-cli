    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/network/vhub/route-map"
    },
    "commands": [
        {
            "name": "az network vhub route-map delete",
            "description": "Delete a route map.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/virtualHubs/{virtualHubName}/routeMaps/{routeMapName}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/network/vhub/route-map#az-network-vhub-route-map-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-group]",
                            "[--name]",
                            "[--route-map-name]",
                            "[--subscription]",
                            "[--vhub-name]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete route map",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.routeMapName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--vhub-name",
                            "value": "[path.virtualHubName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az network vhub route-map show",
