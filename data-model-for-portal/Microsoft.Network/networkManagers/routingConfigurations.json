                    "description": "List all the rule collections.",
                    "parameters": [
                        {
                            "name": "--config-name",
                            "value": "[path.configurationName]"
                        },
                        {
                            "name": "--manager-name",
                            "value": "[path.networkManagerName]"
                        },
                        {
                            "name": "--resource-group",
