{
    "resourceType": "namespaces/wcfRelays",
    "apiVersion": "2017-04-01",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/relay/wcfrelay"
    },
    "commands": [
        {
            "name": "az relay wcfrelay delete",
            "description": "Deletes the Relay Service WCF Relay.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Relay/namespaces/{namespaceName}/wcfRelays/{relayName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/relay/wcfrelay#az-relay-wcfrelay-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--namespace-name]",
                            "[--name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Deletes the wcfrelay",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--namespace-name",
                            "value": "[path.namespaceName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.relayName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az relay wcfrelay show",
            "description": "Shows the Relay Service WCF Relay Details.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Relay/namespaces/{namespaceName}/wcfRelays/{relayName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/relay/wcfrelay#az-relay-wcfrelay-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--namespace-name]",
                            "[--name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Shows the Relay Service WCF Relay Details",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--namespace-name",
                            "value": "[path.namespaceName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.relayName]"
                        },
                        {
