{
    "resourceType": "kubernetesClusters",
    "apiVersion": "2023-10-01-preview",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/networkcloud/kubernetescluster"
    },
    "commands": [
        {
            "name": "az networkcloud kubernetescluster delete",
            "description": "Delete the provided Kubernetes cluster.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.NetworkCloud/kubernetesClusters/{kubernetesClusterName}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/networkcloud/kubernetescluster#az-networkcloud-kubernetescluster-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--kubernetes-cluster-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete Kubernetes cluster",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.kubernetesClusterName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az networkcloud kubernetescluster list",
            "description": "List Kubernetes clusters in the provided subscription.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.NetworkCloud/kubernetesClusters",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/networkcloud/kubernetescluster#az-networkcloud-kubernetescluster-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List Kubernetes clusters for resource group",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az networkcloud kubernetescluster show",
            "description": "Get properties of the provided the Kubernetes cluster.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.NetworkCloud/kubernetesClusters/{kubernetesClusterName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/networkcloud/kubernetescluster#az-networkcloud-kubernetescluster-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--kubernetes-cluster-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Get Kubernetes cluster",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.kubernetesClusterName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az networkcloud kubernetescluster agentpool list",
            "description": "List agent pools for the provided Kubernetes cluster.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.NetworkCloud/kubernetesClusters/{kubernetesClusterName}/agentPools",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/networkcloud/kubernetescluster/agentpool#az-networkcloud-kubernetescluster-agentpool-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--kubernetes-cluster-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List agent pools of the Kubernetes cluster",
                    "parameters": [
                        {
                            "name": "--kubernetes-cluster-name",
                            "value": "[path.kubernetesClusterName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}