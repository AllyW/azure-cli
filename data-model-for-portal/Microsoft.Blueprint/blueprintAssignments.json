            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/blueprint/assignment#az-blueprint-assignment-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--resource-scope]",
                            "[--delete-behavior]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete a blueprint assignment.",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.assignmentName]"
