                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--bookmark-id]",
                            "[--resource-group]",
                            "[--subscription]",
                            "[--workspace-name]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete the bookmark.",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.bookmarkId]"
                        },
