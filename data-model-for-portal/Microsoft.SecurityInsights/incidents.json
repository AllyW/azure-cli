                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--incident-id]",
                            "[--resource-group]",
                            "[--subscription]",
                            "[--workspace-name]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete the incident.",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.incidentId]"
                        },
                        {
                            "name": "--resource-group",
