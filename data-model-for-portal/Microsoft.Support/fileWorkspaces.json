                    "description": "Show a file workspace for a specified subscription (different from the account subscription)",
                    "parameters": [
                        {
                            "name": "--file-workspace-name",
                            "value": "[path.fileWorkspaceName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
