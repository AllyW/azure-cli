{
    "resourceType": "locations",
    "apiVersion": "2024-04-04-preview",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/mdp/sku"
    },
    "commands": [
        {
            "name": "az mdp sku list",
            "description": "List sku resources in given location",
            "path": "/subscriptions/{subscriptionId}/providers/Microsoft.DevOpsInfrastructure/locations/{locationName}/skus",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/mdp/sku#az-mdp-sku-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--location]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List by location",
                    "parameters": [
                        {
                            "name": "--location",
                            "value": "[path.locationName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az mdp usage list",
            "description": "List quota resources in given location",
            "path": "/subscriptions/{subscriptionId}/providers/Microsoft.DevOpsInfrastructure/locations/{location}/usages",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/mdp/usage#az-mdp-usage-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--location]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List by location",
                    "parameters": [
                        {
                            "name": "--location",
                            "value": "[path.location]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}