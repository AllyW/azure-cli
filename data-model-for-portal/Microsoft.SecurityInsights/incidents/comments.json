            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/sentinel/incident/comment#az-sentinel-incident-comment-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--incident-comment-id]",
                            "[--incident-id]",
                            "[--resource-group]",
                            "[--subscription]",
                            "[--workspace-name]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete the incident comment.",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.incidentCommentId]"
                        },
                        {
                            "name": "--incident-id",
                            "value": "[path.incidentId]"
                        },
                        {
                            "name": "--resource-group",
