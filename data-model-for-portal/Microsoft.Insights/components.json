{
    "resourceType": "components",
    "apiVersion": "2015-05-01",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/monitor/app-insights/component/billing"
    },
    "commands": [
        {
            "name": "az monitor app-insights component billing show",
            "description": "Show the billing features of an Application Insights resource.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Insights/components/{resourceName}/currentbillingfeatures",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/monitor/app-insights/component/billing#az-monitor-app-insights-component-billing-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-group]",
                            "[--app]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Show the billing features of an application insights component",
                    "parameters": [
                        {
                            "name": "--app",
                            "value": "[path.resourceName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az monitor app-insights component continues-export list",
            "description": "List a list of Continuous Export configuration of an Application Insights component.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Insights/components/{resourceName}/exportconfiguration",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/monitor/app-insights/component/continues-export#az-monitor-app-insights-component-continues-export-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-group]",
                            "[--app]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List export configurations",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--app",
                            "value": "[path.resourceName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az monitor app-insights component favorite list",
            "description": "List a list of favorites defined within an Application Insights component.",
