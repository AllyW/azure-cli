{
    "resourceType": "nginxDeployments",
    "apiVersion": "2024-01-01-preview",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/nginx/deployment"
    },
    "commands": [
        {
            "name": "az nginx deployment delete",
            "description": "Delete an NGINX deployment",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Nginx.NginxPlus/nginxDeployments/{deploymentName}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/nginx/deployment#az-nginx-deployment-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--deployment-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Deployment Delete",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.deploymentName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az nginx deployment list",
            "description": "List of NGINX deployments",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Nginx.NginxPlus/nginxDeployments",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/nginx/deployment#az-nginx-deployment-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Deployment List",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az nginx deployment show",
