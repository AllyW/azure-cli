{
    "resourceType": "profiles/customDomains",
    "apiVersion": "2024-02-01",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/afd/custom-domain"
    },
    "commands": [
        {
            "name": "az afd custom-domain delete",
            "description": "Delete an existing AzureFrontDoor domain with the specified domain name under the specified subscription, resource group and profile.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Cdn/profiles/{profileName}/customDomains/{customDomainName}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/afd/custom-domain#az-afd-custom-domain-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--custom-domain-name]",
                            "[--profile-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete a custom domain.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--profile-name",
                            "value": "[path.profileName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.customDomainName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az afd custom-domain regenerate-validation-token",
            "description": "Updates the domain validation token.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Cdn/profiles/{profileName}/customDomains/{customDomainName}/refreshValidationToken",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/afd/custom-domain#az-afd-custom-domain-regenerate-validation-token"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--custom-domain-name]",
                            "[--profile-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Regenerate the domain validation token.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--profile-name",
                            "value": "[path.profileName]"
                        },
                        {
                            "name": "--custom-domain-name",
                            "value": "[path.customDomainName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az afd custom-domain show",
            "description": "Get an existing AzureFrontDoor domain with the specified domain name under the specified subscription, resource group and profile.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Cdn/profiles/{profileName}/customDomains/{customDomainName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/afd/custom-domain#az-afd-custom-domain-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--custom-domain-name]",
                            "[--profile-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "show details of the custom domain within the specified profile.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--profile-name",
                            "value": "[path.profileName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.customDomainName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}