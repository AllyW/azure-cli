                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--fabric-name]",
                            "[--resource-group]",
                            "[--vault-name]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "fabric delete",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.fabricName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
