{
    "resourceType": "hostGroups/hosts",
    "apiVersion": "2022-11-01",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/vm/host"
    },
    "commands": [
        {
            "name": "az vm host delete",
            "description": "Delete a dedicated host.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Compute/hostGroups/{hostGroupName}/hosts/{hostName}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/vm/host#az-vm-host-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--host-group]",
                            "[--host-group-name]",
                            "[--name]",
                            "[--host-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete a dedicated host.",
                    "parameters": [
                        {
                            "name": "--host-group",
                            "value": "[path.hostGroupName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.hostName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
