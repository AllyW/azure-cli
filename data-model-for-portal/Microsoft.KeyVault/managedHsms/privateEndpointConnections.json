{
    "resourceType": "managedHsms/privateEndpointConnections",
    "apiVersion": "2021-04-01-preview",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/keyvault/private-endpoint-connection"
    },
    "commands": [
        {
            "name": "az keyvault private-endpoint-connection show",
            "description": "Show details of a private endpoint connection associated with a MHSM",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.KeyVault/managedHSMs/{name}/privateEndpointConnections/{privateEndpointConnectionName}",
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/keyvault/private-endpoint-connection#az-keyvault-private-endpoint-connection-show"
                },
                "parameterSets": [
                    {
                       "parameters": [
                          "[--id]",
                          "[--name]",
                          "[--hsm-name]",
                          "--resource-group",
                          "--subscription"
                       ]
                    }
                 ]
            },
            "examples": [
                {
                    "description": "Show details of a private endpoint connection associated with a managed HSM",
                    "parameters": [
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--hsm-name",
                            "value": "[path.name]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.privateEndpointConnectionName]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az keyvault private-endpoint-connection delete",
            "description": "Delete the specified private endpoint connection associated with a MHSM",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.KeyVault/managedHSMs/{name}/privateEndpointConnections/{privateEndpointConnectionName}",
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/keyvault/private-endpoint-connection#az-keyvault-private-endpoint-connection-delete"
                },
                "parameterSets": [
                    {
                       "parameters": [
                          "[--id]",
                          "[--name]",
                          "[--hsm-name]",
                          "--resource-group",
                          "--subscription",
                          "[--no-wait]"
                       ]
                    }
                 ]
            },
            "examples": [
                {
                    "description": "Delete the specified private endpoint connection associated with a managed HSM",
                    "parameters": [
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--hsm-name",
                            "value": "[path.name]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.privateEndpointConnectionName]"
                        }
                    ]
                }
            ]
        }
    ]
}