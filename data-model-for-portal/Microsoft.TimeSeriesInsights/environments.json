{
    "resourceType": "environments",
    "apiVersion": "2020-05-15",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/tsi/access-policy"
    },
    "commands": [
        {
            "name": "az tsi access-policy list",
            "description": "List all the available access policies associated with the environment.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.TimeSeriesInsights/environments/{environmentName}/accessPolicies",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/tsi/access-policy#az-tsi-access-policy-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--environment-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "AccessPoliciesByEnvironment",
                    "parameters": [
                        {
                            "name": "--environment-name",
                            "value": "[path.environmentName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az tsi environment delete",
            "description": "Delete the environment with the specified name in the specified subscription and resource group.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.TimeSeriesInsights/environments/{environmentName}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/tsi/environment#az-tsi-environment-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--environment-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "EnvironmentsDelete",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.environmentName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az tsi environment list",
            "description": "List all the available environments within a subscription, irrespective of the resource groups.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.TimeSeriesInsights/environments",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/tsi/environment#az-tsi-environment-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "EnvironmentsByResourceGroup",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az tsi environment show",
            "description": "Get the environment with the specified name in the specified subscription and resource group.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.TimeSeriesInsights/environments/{environmentName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/tsi/environment#az-tsi-environment-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--environment-name]",
                            "[--resource-group]",
                            "[--subscription]",
                            "[--expand]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "EnvironmentsGet",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.environmentName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az tsi reference-data-set list",
            "description": "List all the available reference data sets associated with the subscription and within the specified resource group and environment.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.TimeSeriesInsights/environments/{environmentName}/referenceDataSets",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/tsi/reference-data-set#az-tsi-reference-data-set-list"
