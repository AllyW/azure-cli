{
    "resourceType": "logprofiles",
    "apiVersion": "2016-03-01",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/monitor/log-profiles"
    },
    "commands": [
        {
            "name": "az monitor log-profiles delete",
            "description": "Delete the log profile.",
            "path": "/subscriptions/{subscriptionId}/providers/Microsoft.Insights/logprofiles/{logProfileName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/monitor/log-profiles#az-monitor-log-profiles-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete the log profile.",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.logProfileName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az monitor log-profiles list",
            "description": "List the log profiles.",
            "path": "/subscriptions/{subscriptionId}/providers/Microsoft.Insights/logprofiles",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/monitor/log-profiles#az-monitor-log-profiles-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List the log profiles.",
                    "parameters": [
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az monitor log-profiles show",
            "description": "Get the log profile.",
            "path": "/subscriptions/{subscriptionId}/providers/Microsoft.Insights/logprofiles/{logProfileName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/monitor/log-profiles#az-monitor-log-profiles-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Get the log profile.",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.logProfileName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}