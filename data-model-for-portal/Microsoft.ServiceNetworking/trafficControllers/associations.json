            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/network/alb/association#az-network-alb-association-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--association-name]",
                            "[--resource-group]",
                            "[--subscription]",
                            "[--alb-name]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete an Application Gateway for Containers association resource",
                    "parameters": [
                        {
                            "name": "--resource-group",
