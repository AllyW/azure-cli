{
    "resourceType": "metadata",
    "apiVersion": "2022-06-01-preview",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/sentinel/metadata"
    },
    "commands": [
        {
            "name": "az sentinel metadata delete",
            "description": "Delete a Metadata.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.OperationalInsights/workspaces/{workspaceName}/providers/Microsoft.SecurityInsights/metadata/{metadataName}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/sentinel/metadata#az-sentinel-metadata-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--metadata-name]",
                            "[--resource-group]",
                            "[--subscription]",
                            "[--workspace-name]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete a Metadata.",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.metadataName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        },
                        {
                            "name": "--workspace-name",
                            "value": "[path.workspaceName]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az sentinel metadata list",
            "description": "List of all metadata",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.OperationalInsights/workspaces/{workspaceName}/providers/Microsoft.SecurityInsights/metadata",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/sentinel/metadata#az-sentinel-metadata-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-group]",
                            "[--subscription]",
                            "[--workspace-name]",
                            "[--filter]",
                            "[--orderby]",
                            "[--skip]",
                            "[--top]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List of all metadata",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        },
                        {
                            "name": "--workspace-name",
                            "value": "[path.workspaceName]"
                        }
                    ]
                }
            ]
        },
        {
