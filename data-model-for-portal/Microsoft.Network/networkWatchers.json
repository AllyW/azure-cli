{
    "resourceType": "networkWatchers",
    "apiVersion": "2022-01-01",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/network/watcher"
    },
    "commands": [
        {
            "name": "az network watcher list",
            "description": "List Network Watchers.",
            "path": "/subscriptions/{subscriptionId}/providers/Microsoft.Network/networkWatchers",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/network/watcher#az-network-watcher-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List Network Watchers.",
                    "parameters": [
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az network watcher connection-monitor list",
            "description": "List connection monitors for the given region.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/networkWatchers/{networkWatcherName}/connectionMonitors",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/network/watcher/connection-monitor#az-network-watcher-connection-monitor-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--network-watcher-name]",
                            "[--resource-group-name]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List connection monitors for the given region.",
                    "parameters": [
                        {
                            "name": "--network-watcher-name",
                            "value": "[path.networkWatcherName]"
                        },
                        {
                            "name": "--resource-group-name",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az network watcher flow-log list",
            "description": "List all flow log resources for the specified Network Watcher.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/networkWatchers/{networkWatcherName}/flowLogs",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/network/watcher/flow-log#az-network-watcher-flow-log-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--network-watcher-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List all flow log resources for the specified Network Watcher.",
                    "parameters": [
                        {
                            "name": "--network-watcher-name",
                            "value": "[path.networkWatcherName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az network watcher packet-capture list",
            "description": "List all packet capture sessions within a region.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/networkWatchers/{networkWatcherName}/packetCaptures",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/network/watcher/packet-capture#az-network-watcher-packet-capture-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--watcher-name]",
                            "[--watcher-rg]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List all packet capture sessions within a region.",
                    "parameters": [
                        {
                            "name": "--watcher-name",
                            "value": "[path.networkWatcherName]"
                        },
                        {
                            "name": "--watcher-rg",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}