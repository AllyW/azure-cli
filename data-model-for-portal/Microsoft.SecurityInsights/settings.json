                    "url": "https://docs.microsoft.com/cli/azure/sentinel/setting#az-sentinel-setting-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-group]",
                            "[--name]",
                            "[--settings-name]",
                            "[--subscription]",
                            "[--workspace-name]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Get a setting.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.settingsName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        },
                        {
                            "name": "--workspace-name",
                            "value": "[path.workspaceName]"
                        }
                    ]
                }
            ]
