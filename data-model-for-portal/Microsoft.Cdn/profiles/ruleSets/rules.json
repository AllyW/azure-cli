{
    "resourceType": "profiles/ruleSets/rules",
    "apiVersion": "2024-02-01",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/afd/rule"
    },
    "commands": [
        {
            "name": "az afd rule delete",
            "description": "Delete an existing delivery rule within a rule set.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Cdn/profiles/{profileName}/ruleSets/{ruleSetName}/rules/{ruleName}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/afd/rule#az-afd-rule-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--profile-name]",
                            "[--resource-group]",
                            "[--name]",
                            "[--rule-name]",
                            "[--rule-set-name]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Remove a rule with name rule1.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--rule-set-name",
                            "value": "[path.ruleSetName]"
                        },
                        {
                            "name": "--profile-name",
                            "value": "[path.profileName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.ruleName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az afd rule show",
            "description": "Get an existing delivery rule within a rule set.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Cdn/profiles/{profileName}/ruleSets/{ruleSetName}/rules/{ruleName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/afd/rule#az-afd-rule-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--profile-name]",
                            "[--resource-group]",
                            "[--name]",
                            "[--rule-name]",
                            "[--rule-set-name]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "show details of the delivery rule with name rule1.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--rule-set-name",
                            "value": "[path.ruleSetName]"
                        },
                        {
                            "name": "--profile-name",
                            "value": "[path.profileName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.ruleName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}