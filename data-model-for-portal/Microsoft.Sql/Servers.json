{
    "resourceType": "Servers",
    "apiVersion": "2021-11-01",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/sql/server"
    },
    "commands": [
        {
            "name": "az sql server delete",
            "description": "Deletes a server.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Sql/servers/{serverName}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/sql/server#az-sql-server-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--ids]",
                            "[--name]",
                            "[--resource-group]",
                            "[--subscription]",
                            "[--yes]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete a sql server",
                    "parameters": [
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.serverName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az sql server list",
            "description": "List available servers.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Sql/servers",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/sql/server#az-sql-server-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--expand-ad-admin]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List all servers in a resource group.",
                    "parameters": [
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az sql server show",
            "description": "Gets a server.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Sql/servers/{serverName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/sql/server#az-sql-server-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--expand-ad-admin]",
                            "[--ids]",
                            "[--name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Get the details for a sql server",
                    "parameters": [
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.serverName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az sql server firewall-rule list",
            "description": "List a server's firewall rules.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Sql/servers/{serverName}/firewallRules",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/sql/server/firewall-rule#az-sql-server-firewall-rule-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--ids]",
                            "[--resource-group]",
                            "[--server]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List a server's firewall rules",
                    "parameters": [
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--server",
                            "value": "[path.serverName]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az sql server vnet-rule list",
            "description": "Gets a list of virtual network rules in a server.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Sql/servers/{serverName}/virtualNetworkRules",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/sql/server/vnet-rule#az-sql-server-vnet-rule-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--ids]",
                            "[--resource-group]",
                            "[--server]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List a server's virtual network rules",
                    "parameters": [
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--server",
                            "value": "[path.serverName]"
                        }
                    ]
                }
            ]
        }
    ]
}