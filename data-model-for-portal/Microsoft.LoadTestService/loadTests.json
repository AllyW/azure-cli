                    }
                ]
            },
            "examples": [
                {
                    "description": "Show an Azure load testing resource",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.loadTestName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
