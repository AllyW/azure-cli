{
    "resourceType": "profiles/endpoints/customDomains",
    "apiVersion": "2024-02-01",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/cdn/custom-domain"
    },
    "commands": [
        {
            "name": "az cdn custom-domain delete",
            "description": "Delete an existing custom domain within an endpoint.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Cdn/profiles/{profileName}/endpoints/{endpointName}/customDomains/{customDomainName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/cdn/custom-domain#az-cdn-custom-domain-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--custom-domain-name]",
                            "[--endpoint-name]",
                            "[--profile-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete the custom domain of a CDN.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--endpoint-name",
                            "value": "[path.endpointName]"
                        },
                        {
                            "name": "--profile-name",
                            "value": "[path.profileName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.customDomainName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az cdn custom-domain disable-https",
            "description": "Disable https delivery of the custom domain.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Cdn/profiles/{profileName}/endpoints/{endpointName}/customDomains/{customDomainName}/disableCustomHttps",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/cdn/custom-domain#az-cdn-custom-domain-disable-https"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--custom-domain-name]",
                            "[--endpoint-name]",
                            "[--profile-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Disable https delivery of the custom domain.",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.customDomainName]"
                        },
                        {
                            "name": "--endpoint-name",
                            "value": "[path.endpointName]"
                        },
                        {
                            "name": "--profile-name",
                            "value": "[path.profileName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az cdn custom-domain show",
            "description": "Get an existing custom domain within an endpoint.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Cdn/profiles/{profileName}/endpoints/{endpointName}/customDomains/{customDomainName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/cdn/custom-domain#az-cdn-custom-domain-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--custom-domain-name]",
                            "[--endpoint-name]",
                            "[--profile-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Get the details of a custom domain with resource name customdomain1.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--endpoint-name",
                            "value": "[path.endpointName]"
                        },
                        {
                            "name": "--profile-name",
                            "value": "[path.profileName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.customDomainName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}