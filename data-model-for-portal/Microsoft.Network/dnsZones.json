                "parameterSets": [
                    {
                        "parameters": [
                            "[--if-match]",
                            "[--resource-group]",
                            "[--subscription]",
                            "[--name]",
                            "[--zone-name]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete a DNS zone using a fully qualified domain name.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.zoneName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az network dns zone list",
            "description": "List the DNS zones.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/dnsZones",
            "confirmation": false,
            "help": {
