{
    "resourceType": "locations",
    "apiVersion": "2022-09-04",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/cloud-service/os-family"
    },
    "commands": [
        {
            "name": "az cloud-service os-family list",
            "description": "List all guest operating system families available to be specified in the XML service configuration (.cscfg) for a cloud service.",
            "path": "/subscriptions/{subscriptionId}/providers/Microsoft.Compute/locations/{location}/cloudServiceOsFamilies",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/cloud-service/os-family#az-cloud-service-os-family-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--location]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List os-family",
                    "parameters": [
                        {
                            "name": "--location",
                            "value": "[path.location]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az cloud-service os-version list",
            "description": "List all guest operating system versions available to be specified in the XML service configuration (.cscfg) for a cloud service.",
            "path": "/subscriptions/{subscriptionId}/providers/Microsoft.Compute/locations/{location}/cloudServiceOsVersions",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/cloud-service/os-version#az-cloud-service-os-version-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--location]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List os version",
                    "parameters": [
                        {
                            "name": "--location",
                            "value": "[path.location]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}