{
    "resourceType": "elasticSans/volumegroups/snapshots",
    "apiVersion": "2023-01-01",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/elastic-san/volume/snapshot"
    },
    "commands": [
        {
            "name": "az elastic-san volume snapshot delete",
            "description": "Delete a Volume Snapshot.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ElasticSan/elasticSans/{elasticSanName}/volumegroups/{volumeGroupName}/snapshots/{snapshotName}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/elastic-san/volume/snapshot#az-elastic-san-volume-snapshot-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--elastic-san]",
                            "[--elastic-san-name]",
                            "[--resource-group]",
                            "[--name]",
                            "[--snapshot-name]",
                            "[--subscription]",
                            "[--volume-group]",
                            "[--volume-group-name]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "snapshot delete",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--elastic-san",
                            "value": "[path.elasticSanName]"
                        },
                        {
                            "name": "--volume-group",
                            "value": "[path.volumeGroupName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.snapshotName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az elastic-san volume snapshot show",
            "description": "Get a Volume Snapshot.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ElasticSan/elasticSans/{elasticSanName}/volumegroups/{volumeGroupName}/snapshots/{snapshotName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/elastic-san/volume/snapshot#az-elastic-san-volume-snapshot-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--elastic-san]",
                            "[--elastic-san-name]",
                            "[--resource-group]",
                            "[--name]",
                            "[--snapshot-name]",
                            "[--subscription]",
                            "[--volume-group]",
                            "[--volume-group-name]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "snapshot show",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--elastic-san",
                            "value": "[path.elasticSanName]"
                        },
                        {
                            "name": "--volume-group",
                            "value": "[path.volumeGroupName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.snapshotName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}