{
    "resourceType": "networkManagers/networkGroups",
    "apiVersion": "2022-01-01",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/network/manager/group"
    },
    "commands": [
        {
            "name": "az network manager group delete",
            "description": "Delete a network group.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/networkManagers/{networkManagerName}/networkGroups/{networkGroupName}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/network/manager/group#az-network-manager-group-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--network-group-name]",
                            "[--network-manager-name]",
                            "[--resource-group]",
                            "[--subscription]",
                            "[--force]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete a network group.",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.networkGroupName]"
                        },
                        {
                            "name": "--network-manager-name",
                            "value": "[path.networkManagerName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az network manager group show",
            "description": "Get the specified network group.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/networkManagers/{networkManagerName}/networkGroups/{networkGroupName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/network/manager/group#az-network-manager-group-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--network-group-name]",
                            "[--network-manager-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Get Azure Virtual Network Manager Network Group",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.networkGroupName]"
                        },
                        {
                            "name": "--network-manager-name",
                            "value": "[path.networkManagerName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az network manager group static-member list",
            "description": "List the static members within a specific network group.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/networkManagers/{networkManagerName}/networkGroups/{networkGroupName}/staticMembers",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/network/manager/group/static-member#az-network-manager-group-static-member-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--network-group]",
                            "[--network-group-name]",
                            "[--network-manager]",
                            "[--network-manager-name]",
                            "[--resource-group]",
                            "[--subscription]",
                            "[--skip-token]",
                            "[--top]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List static member in a network manager group",
                    "parameters": [
                        {
                            "name": "--network-manager",
                            "value": "[path.networkManagerName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--network-group",
                            "value": "[path.networkGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}