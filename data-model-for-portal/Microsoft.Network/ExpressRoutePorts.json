{
    "resourceType": "expressRoutePorts",
    "apiVersion": "2023-09-01",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/network/express-route/port/authorization"
    },
    "commands": [
        {
            "name": "az network express-route port authorization list",
            "description": "List all authorizations in an express route port.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/expressRoutePorts/{expressRoutePortName}/authorizations",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/network/express-route/port/authorization#az-network-express-route-port-authorization-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--port-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List all authorizations in an express route port.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--port-name",
                            "value": "[path.expressRoutePortName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}