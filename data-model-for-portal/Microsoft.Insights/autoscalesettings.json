{
    "resourceType": "autoscalesettings",
    "apiVersion": "2022-10-01",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/monitor/autoscale"
    },
    "commands": [
        {
            "name": "az monitor autoscale delete",
            "description": "Delete an autoscale setting",
            "path": "/subscriptions/{subscriptionId}/resourcegroups/{resourceGroupName}/providers/Microsoft.Insights/autoscalesettings/{autoscaleSettingName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/monitor/autoscale#az-monitor-autoscale-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--autoscale-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete an autoscale setting",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.autoscaleSettingName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az monitor autoscale list",
            "description": "Lists the autoscale settings for a resource group",
            "path": "/subscriptions/{subscriptionId}/resourcegroups/{resourceGroupName}/providers/Microsoft.Insights/autoscalesettings",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/monitor/autoscale#az-monitor-autoscale-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Lists the autoscale settings for a resource group",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az monitor autoscale show",
            "description": "Get an autoscale setting",
            "path": "/subscriptions/{subscriptionId}/resourcegroups/{resourceGroupName}/providers/Microsoft.Insights/autoscalesettings/{autoscaleSettingName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/monitor/autoscale#az-monitor-autoscale-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--autoscale-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Show autoscale setting details.",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.autoscaleSettingName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}