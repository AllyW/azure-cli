            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/relay/wcfrelay/authorization-rule#az-relay-wcfrelay-authorization-rule-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--namespace-name]",
                            "[--relay-name]",
                            "[--resource-group]",
                            "[--subscription]"
