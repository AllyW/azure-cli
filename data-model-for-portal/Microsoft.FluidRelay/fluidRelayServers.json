    },
    "commands": [
        {
            "name": "az fluid-relay container list",
            "description": "List all Fluid Relay containers which are children of a given Fluid Relay server.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroup}/providers/Microsoft.FluidRelay/fluidRelayServers/{fluidRelayServerName}/fluidRelayContainers",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/fluid-relay/container#az-fluid-relay-container-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--server-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "FluidRelayContainer_List",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroup]"
                        },
                        {
                            "name": "--server-name",
                            "value": "[path.fluidRelayServerName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
