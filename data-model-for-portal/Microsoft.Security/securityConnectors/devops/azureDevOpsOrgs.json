{
    "resourceType": "securityConnectors/devops/azureDevOpsOrgs",
    "apiVersion": "2023-09-01-preview",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/security/security-connector/devops/azuredevopsorg"
    },
    "commands": [
        {
            "name": "az security security-connector devops azuredevopsorg show",
            "description": "Get a monitored Azure DevOps organization resource.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Security/securityConnectors/{securityConnectorName}/devops/default/azureDevOpsOrgs/{orgName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/security/security-connector/devops/azuredevopsorg#az-security-security-connector-devops-azuredevopsorg-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--org-name]",
                            "[--resource-group]",
                            "[--name]",
                            "[--security-connector-name]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Get a monitored Azure DevOps organization resource.",
                    "parameters": [
                        {
                            "name": "--org-name",
                            "value": "[path.orgName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.securityConnectorName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az security security-connector devops azuredevopsorg project list",
            "description": "List Azure DevOps projects onboarded to the connector.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Security/securityConnectors/{securityConnectorName}/devops/default/azureDevOpsOrgs/{orgName}/projects",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/security/security-connector/devops/azuredevopsorg/project#az-security-security-connector-devops-azuredevopsorg-project-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--org-name]",
                            "[--resource-group]",
                            "[--name]",
                            "[--security-connector-name]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List Azure DevOps projects onboarded to the connector.",
                    "parameters": [
                        {
                            "name": "--org-name",
                            "value": "[path.orgName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.securityConnectorName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}