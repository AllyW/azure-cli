            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/site-recovery/protected-item#az-site-recovery-protected-item-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--fabric-name]",
                            "[--protection-container]",
                            "[--protection-container-name]",
                            "[--name]",
                            "[--replicated-protected-item-name]",
                            "[--resource-group]",
                            "[--vault-name]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "protected-item delete",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--fabric-name",
                            "value": "[path.fabricName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.replicatedProtectedItemName]"
                        },
                        {
                            "name": "--protection-container",
                            "value": "[path.protectionContainerName]"
                        },
                        {
