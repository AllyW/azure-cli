                        },
                        {
                            "name": "--namespace-name",
                            "value": "[path.namespaceName]"
                        },
                        {
                            "name": "--queue-name",
                            "value": "[path.queueName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.authorizationRuleName]"
                        },
                        {
                            "name": "--subscription",
