{
    "resourceType": "dnsZones",
    "apiVersion": "2023-07-01-preview",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/network/dns/dnssec-config"
    },
    "commands": [
        {
            "name": "az network dns dnssec-config create",
            "description": "Create the DNSSEC configuration on a DNS zone.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/dnsZones/{zoneName}/dnssecConfigs/default",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/network/dns/dnssec-config#az-network-dns-dnssec-config-create"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--if-match]",
                            "[--if-none-match]",
                            "[--resource-group]",
                            "[--subscription]",
                            "[--zone-name]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Enable DNSSEC on a zone.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--zone-name",
                            "value": "[path.zoneName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az network dns dnssec-config delete",
            "description": "Delete the DNSSEC configuration on a DNS zone. This operation cannot be undone.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/dnsZones/{zoneName}/dnssecConfigs/default",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/network/dns/dnssec-config#az-network-dns-dnssec-config-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--if-match]",
                            "[--resource-group]",
                            "[--subscription]",
                            "[--zone-name]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Disable DNSSEC on a zone.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--zone-name",
                            "value": "[path.zoneName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az network dns dnssec-config show",
            "description": "Get the DNSSEC configuration.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/dnsZones/{zoneName}/dnssecConfigs/default",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/network/dns/dnssec-config#az-network-dns-dnssec-config-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-group]",
                            "[--subscription]",
                            "[--zone-name]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Get the DNSSEC configuration on a zone.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--zone-name",
                            "value": "[path.zoneName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az network dns record-set list",
            "description": "List all record sets within a DNS zone.",
