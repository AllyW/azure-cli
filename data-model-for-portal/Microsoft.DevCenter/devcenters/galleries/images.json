            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/devcenter/admin/image#az-devcenter-admin-image-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--dev-center]",
                            "[--dev-center-name]",
                            "[--gallery-name]",
                            "[--name]",
                            "[--image-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Get",
                    "parameters": [
                        {
                            "name": "--dev-center",
                            "value": "[path.devCenterName]"
                        },
                        {
                            "name": "--gallery-name",
                            "value": "[path.galleryName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.imageName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az devcenter admin image-version list",
