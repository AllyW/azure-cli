                    "description": "Get a Volume Group.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--elastic-san",
                            "value": "[path.elasticSanName]"
