                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--endpoint-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete a private endpoint dns zone group.",
                    "parameters": [
                        {
                            "name": "--endpoint-name",
                            "value": "[path.privateEndpointName]"
