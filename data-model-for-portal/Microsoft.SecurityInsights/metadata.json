            "description": "Get a Metadata.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.OperationalInsights/workspaces/{workspaceName}/providers/Microsoft.SecurityInsights/metadata/{metadataName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/sentinel/metadata#az-sentinel-metadata-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--metadata-name]",
                            "[--resource-group]",
                            "[--subscription]",
                            "[--workspace-name]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Get a Metadata.",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.metadataName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        },
                        {
                            "name": "--workspace-name",
                            "value": "[path.workspaceName]"
                        }
                    ]
                }
            ]
        }
    ]
