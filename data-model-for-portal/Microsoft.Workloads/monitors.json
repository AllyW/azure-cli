                    }
                ]
            },
            "examples": [
                {
                    "description": "Show properties of a SAP monitor for the specified subscription, resource group, and resource name.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.monitorName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az workloads monitor provider-instance list",
            "description": "List a list of provider instances in the specified SAP monitor. The operations returns various properties of each provider instances.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Workloads/monitors/{monitorName}/providerInstances",
            "confirmation": false,
            "help": {
