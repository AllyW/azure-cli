{
    "resourceType": "backupVaults/backupJobs",
    "apiVersion": "2024-04-01",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/dataprotection/job"
    },
    "commands": [
        {
            "name": "az dataprotection job show",
            "description": "Get a job with id in a backup vault.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.DataProtection/backupVaults/{vaultName}/backupJobs/{jobId}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/dataprotection/job#az-dataprotection-job-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-group]",
                            "[--subscription]",
                            "[--vault-name]",
                            "[--job-id]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Get Job",
                    "parameters": [
                        {
                            "name": "--job-id",
                            "value": "[path.jobId]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--vault-name",
                            "value": "[path.vaultName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}