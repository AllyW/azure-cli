{
    "resourceType": "services/workspaces",
    "apiVersion": "2024-03-01",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/apic/api"
    },
    "commands": [
        {
            "name": "az apic api list",
            "description": "List a collection of APIs.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ApiCenter/services/{serviceName}/workspaces/{workspaceName}/apis",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/apic/api#az-apic-api-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-group]",
                            "[--service-name]",
                            "[--subscription]",
                            "[--workspace]",
                            "[--workspace-name]",
                            "[--filter]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List a collection of APIs.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--service-name",
                            "value": "[path.serviceName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        },
                        {
                            "name": "--workspace",
                            "value": "[path.workspaceName]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az apic environment list",
            "description": "List a collection of environments.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ApiCenter/services/{serviceName}/workspaces/{workspaceName}/environments",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/apic/environment#az-apic-environment-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-group]",
                            "[--service-name]",
                            "[--subscription]",
                            "[--workspace]",
                            "[--workspace-name]",
                            "[--filter]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List a collection of environments.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--service-name",
                            "value": "[path.serviceName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        },
                        {
                            "name": "--workspace",
                            "value": "[path.workspaceName]"
                        }
                    ]
                }
            ]
