{
    "resourceType": "galleries",
    "apiVersion": "2021-10-01",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/sig"
    },
    "commands": [
        {
            "name": "az sig delete",
            "description": "Delete a Shared Image Gallery.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Compute/galleries/{galleryName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/sig#az-sig-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--gallery-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete a Shared Image Gallery.",
                    "parameters": [
                        {
                            "name": "--gallery-name",
                            "value": "[path.galleryName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az sig gallery-application list",
            "description": "List gallery Application Definitions in a gallery.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Compute/galleries/{galleryName}/applications",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/sig/gallery-application#az-sig-gallery-application-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--gallery-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List gallery Application Definitions in a gallery.",
                    "parameters": [
                        {
                            "name": "--gallery-name",
                            "value": "[path.galleryName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az sig image-definition list",
            "description": "List gallery image definitions in a gallery.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Compute/galleries/{galleryName}/images",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/sig/image-definition#az-sig-image-definition-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--gallery-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List gallery image definitions in a gallery.",
                    "parameters": [
                        {
                            "name": "--gallery-name",
                            "value": "[path.galleryName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}