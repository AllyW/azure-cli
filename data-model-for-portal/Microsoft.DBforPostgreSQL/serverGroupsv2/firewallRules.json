{
    "resourceType": "serverGroupsv2/firewallRules",
    "apiVersion": "2022-11-08",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/cosmosdb/postgres/firewall-rule"
    },
    "commands": [
        {
            "name": "az cosmosdb postgres firewall-rule delete",
            "description": "Delete a cluster firewall rule.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.DBforPostgreSQL/serverGroupsv2/{clusterName}/firewallRules/{firewallRuleName}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/cosmosdb/postgres/firewall-rule#az-cosmosdb-postgres-firewall-rule-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--cluster-name]",
                            "[--name]",
                            "[--firewall-rule-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete the firewall rule of the cluster",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.firewallRuleName]"
                        },
                        {
                            "name": "--cluster-name",
                            "value": "[path.clusterName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az cosmosdb postgres firewall-rule show",
            "description": "Get information about a cluster firewall rule.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.DBforPostgreSQL/serverGroupsv2/{clusterName}/firewallRules/{firewallRuleName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/cosmosdb/postgres/firewall-rule#az-cosmosdb-postgres-firewall-rule-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--cluster-name]",
                            "[--name]",
                            "[--firewall-rule-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Show the firewall rule of the cluster",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.firewallRuleName]"
                        },
                        {
                            "name": "--cluster-name",
                            "value": "[path.clusterName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}