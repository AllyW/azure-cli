{
    "resourceType": "namespaces/eventhubs",
    "apiVersion": "2023-01-01-preview",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/eventhubs/eventhub"
    },
    "commands": [
        {
            "name": "az eventhubs eventhub delete",
            "description": "Delete an Event Hub from the specified Namespace and resource group.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.EventHub/namespaces/{namespaceName}/eventhubs/{eventHubName}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/eventhubs/eventhub#az-eventhubs-eventhub-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--event-hub-name]",
                            "[--namespace-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete an Event Hub from the specified Namespace and resource group.",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.eventHubName]"
                        },
                        {
                            "name": "--namespace-name",
                            "value": "[path.namespaceName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az eventhubs eventhub show",
            "description": "Get an Event Hubs description for the specified Event Hub.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.EventHub/namespaces/{namespaceName}/eventhubs/{eventHubName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/eventhubs/eventhub#az-eventhubs-eventhub-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--event-hub-name]",
                            "[--namespace-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Get an Event Hubs description for the specified Event Hub.",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.eventHubName]"
                        },
                        {
                            "name": "--namespace-name",
                            "value": "[path.namespaceName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az eventhubs eventhub authorization-rule list",
            "description": "List the authorization rules for an Event Hub.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.EventHub/namespaces/{namespaceName}/eventhubs/{eventHubName}/authorizationRules",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/eventhubs/eventhub/authorization-rule#az-eventhubs-eventhub-authorization-rule-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--eventhub-name]",
                            "[--namespace-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "shows the list of Authorization-rules by Eventhub",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--namespace-name",
                            "value": "[path.namespaceName]"
                        },
                        {
                            "name": "--eventhub-name",
                            "value": "[path.eventHubName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az eventhubs eventhub consumer-group list",
            "description": "List all the consumer groups in a Namespace. An empty feed is returned if no consumer group exists in the Namespace.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.EventHub/namespaces/{namespaceName}/eventhubs/{eventHubName}/consumergroups",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/eventhubs/eventhub/consumer-group#az-eventhubs-eventhub-consumer-group-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--eventhub-name]",
                            "[--namespace-name]",
                            "[--resource-group]",
                            "[--subscription]",
                            "[--skip]",
                            "[--top]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List all the consumer groups in a Namespace. An empty feed is returned if no consumer group exists in the Namespace.",
                    "parameters": [
                        {
                            "name": "--eventhub-name",
                            "value": "[path.eventHubName]"
                        },
                        {
                            "name": "--namespace-name",
                            "value": "[path.namespaceName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}