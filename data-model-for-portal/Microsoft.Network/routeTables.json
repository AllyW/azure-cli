{
    "resourceType": "routeTables",
    "apiVersion": "2021-08-01",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/network/route-table"
    },
    "commands": [
        {
            "name": "az network route-table delete",
            "description": "Delete a route table.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/routeTables/{routeTableName}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/network/route-table#az-network-route-table-delete"
                },
                "parameterSets": {
                    "parameters": [
                        "[--resource-group]",
                        "[--name]",
                        "[--subscription]"
                    ]
                }
            },
            "examples": [
                {
                    "description": "Delete a route table.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.routeTableName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az network route-table list",
            "description": "List route tables.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/routeTables",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/network/route-table#az-network-route-table-list"
                },
                "parameterSets": {
                    "parameters": [
                        "[--resource-group]",
                        "[--subscription]"
                    ]
                }
            },
            "examples": [
                {
                    "description": "List all route tables in a subscription.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az network route-table show",
            "description": "Get the details of a route table.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/routeTables/{routeTableName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/network/route-table#az-network-route-table-show"
                },
                "parameterSets": {
                    "parameters": [
                        "[--resource-group]",
                        "[--name]",
                        "[--subscription]",
                        "[--expand]"
                    ]
                }
            },
            "examples": [
                {
                    "description": "Get the details of a route table.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.routeTableName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az network route-table route list",
            "description": "List routes in a route table.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/routeTables/{routeTableName}/routes",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/network/route-table/route#az-network-route-table-route-list"
                },
                "parameterSets": {
                    "parameters": [
                        "[--resource-group]",
                        "[--route-table-name]",
                        "[--subscription]"
                    ]
                }
            },
            "examples": [
                {
                    "description": "List routes in a route table.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--route-table-name",
                            "value": "[path.routeTableName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}