{
    "resourceType": "monitors/providerInstances",
    "apiVersion": "2023-04-01",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/workloads/monitor/provider-instance"
    },
    "commands": [
        {
            "name": "az workloads monitor provider-instance delete",
            "description": "Delete a provider instance for the specified subscription, resource group, SAP monitor name, and resource name.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Workloads/monitors/{monitorName}/providerInstances/{providerInstanceName}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/workloads/monitor/provider-instance#az-workloads-monitor-provider-instance-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--monitor-name]",
                            "[--name]",
                            "[--provider-instance-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete a provider instance for the specified subscription, resource group, SAP monitor name, and resource name.",
                    "parameters": [
                        {
                            "name": "--monitor-name",
                            "value": "[path.monitorName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.providerInstanceName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az workloads monitor provider-instance show",
            "description": "Show properties of a provider instance for the specified subscription, resource group, SAP monitor name, and resource name.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Workloads/monitors/{monitorName}/providerInstances/{providerInstanceName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/workloads/monitor/provider-instance#az-workloads-monitor-provider-instance-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--monitor-name]",
                            "[--name]",
                            "[--provider-instance-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Show properties of a provider instance for the specified subscription, resource group, SAP monitor name, and resource name.",
                    "parameters": [
                        {
                            "name": "--monitor-name",
                            "value": "[path.monitorName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.providerInstanceName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}