{
    "resourceType": "managedClusters",
    "apiVersion": "2023-01-01",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/aks"
    },
    "commands": [
        {
            "name": "az aks list",
            "description": "List managed Kubernetes clusters.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ContainerService/managedClusters",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/aks#az-aks-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Lists managed clusters in the specified subscription and resource group.",
                    "parameters": [
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az aks show",
            "description": "Show the details for a managed Kubernetes cluster.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ContainerService/managedClusters/{resourceName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/aks#az-aks-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "--name",
                            "--resource-group",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Show the details for a managed Kubernetes cluster",
                    "parameters": [
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.resourceName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az aks delete",
            "description": "Delete a managed Kubernetes cluster.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ContainerService/managedClusters/{resourceName}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/aks#az-aks-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "--name",
                            "--resource-group",
                            "[--no-wait]",
                            "[--yes]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete a managed Kubernetes cluster.",
                    "parameters": [
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.resourceName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az aks browse",
            "description": "Show the dashboard for a Kubernetes cluster in a web browser.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ContainerService/managedClusters/{resourceName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/aks#az-aks-browse"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "--name",
                            "--resource-group",
                            "[--disable-browser]",
                            "[--listen-address]",
                            "[--listen-port]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Show the dashboard for a Kubernetes cluster in a web browser.",
                    "parameters": [
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.resourceName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az aks operation-abort",
            "description": "Abort last running operation on managed cluster.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ContainerService/managedClusters/{resourceName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/aks#az-aks-operation-abort"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "--name",
                            "--resource-group",
                            "[--aks-custom-headers]",
                            "[--no-wait]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Abort operation on managed cluster",
                    "parameters": [
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.resourceName]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az aks pod-identity list",
            "description": "List pod identities in a managed Kubernetes cluster.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ContainerService/managedClusters/{resourceName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/aks/pod-identity#az-aks-pod-identity-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "--cluster-name",
                            "--resource-group",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List Pod IDs in a managed Kubernetes cluster.",
                    "parameters": [
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        },
                        {
                            "name": "--cluster-name",
                            "value": "[path.resourceName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az aks addon list",
            "description": "List status of all Kubernetes addons in given cluster.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ContainerService/managedClusters/{resourceName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/aks/addon#az-aks-addon-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "--name",
                            "--resource-group",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List status of all Kubernetes addons in given cluster.",
                    "parameters": [
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.resourceName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        }
                    ]
                }
            ]
        }
    ]
}