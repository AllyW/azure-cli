{
    "resourceType": "vaults",
    "apiVersion": "2021-06-01-preview",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/keyvault"
    },
    "commands": [
        {
            "name": "az keyvault show",
            "description": "Show details of a vault",
            "path":"/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.KeyVault/vaults/{vaultName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/keyvault#az-keyvault-show"
                },
                "parameterSets":[
                    {
                        "parameters": [
                            "--name",
                            "--resource-group",
                            "--subscription"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Show details of a key vault in your resource group.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.vaultName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az keyvault delete",
            "description": "Delete a vault",
            "path":"/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.KeyVault/vaults/{vaultName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/keyvault#az-keyvault-delete"
                },
                "parameterSets":[
                    {
                        "parameters": [
                            "--name",
                            "--resource-group",
                            "--subscription",
                            "[--no-wait]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete specific vault.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.vaultName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az keyvault list",
            "description": "List vaults within specified resource group",
            "path":"/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.KeyVault/vaults",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/keyvault#az-keyvault-list"
                },
                "parameterSets":[
                    {
                        "parameters": [
                            "--resource-group",
                            "--resource-type",
                            "--subscription"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List vaults in your resource group.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--resource-type",
                            "value": "vault"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az keyvault list-deleted",
            "description": "List deleted vaults in a subscription",
            "path":"/subscriptions/{subscriptionId}/providers/Microsoft.KeyVault/deletedVaults",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/keyvault#az-keyvault-list-deleted"
                },
                "parameterSets":[
                    {
                        "parameters": [
                            "--resource-type",
                            "--subscription"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List deleted vaults in a subscription.",
                    "parameters": [
                        {
                            "name": "--resource-type",
                            "value": "vault"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az keyvault show-deleted",
            "description": "Show details of a deleted vault",
            "path":"/subscriptions/{subscriptionId}/providers/Microsoft.KeyVault/locations/{location}/deletedVaults/{vaultName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/keyvault#az-keyvault-show-deleted"
                },
                "parameterSets":[
                    {
                        "parameters": [
                            "--name",
                            "--location",
                            "--subscription"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Show details of a deleted vault.",
                    "parameters": [
                        {
                            "name": "--location",
                            "value": "[path.location]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.vaultName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az keyvault purge",
            "description": "Permanently delete a Vault",
            "path":"/subscriptions/{subscriptionId}/providers/Microsoft.KeyVault/locations/{location}/deletedVaults/{vaultName}/purge",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/keyvault#az-keyvault-purge"
                },
                "parameterSets":[
                    {
                        "parameters": [
                            "--name",
                            "--location",
                            "--subscription",
                            "[--no-wait]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Permanently delete the specified Vault.",
                    "parameters": [
                        {
                            "name": "--location",
                            "value": "[path.location]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.vaultName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az keyvault network-rule list",
            "description": "List the network rules from the network ACLs for a Key Vault",
            "path":"/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.KeyVault/vaults/{vaultName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/keyvault/network-rule#az-keyvault-network-rule-list"
                },
                "parameterSets":[
                    {
                        "parameters": [
                            "--name",
                            "--resource-group",
                            "--subscription"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List the network rules for the specified Key Vault.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.vaultName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az keyvault private-link-resource list",
            "description": "List the private link resources supported for a Key Vault",
            "path":"/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.KeyVault/vaults/{vaultName}/privateLinkResources",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/keyvault/private-link-resource#az-keyvault-private-link-resource-list"
                },
                "parameterSets":[
                    {
                        "parameters": [
                            "--vault-name",
                            "--resource-group",
                            "--subscription"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List the private link resources supported for the key vault.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--vault-name",
                            "value": "[path.vaultName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}