{
    "resourceType": "services",
    "apiVersion": "2024-03-01",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/apic"
    },
    "commands": [
        {
            "name": "az apic delete",
            "description": "Deletes an instance of an Azure API Center service.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ApiCenter/services/{serviceName}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/apic#az-apic-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-group]",
                            "[--name]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete service",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.serviceName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az apic list",
            "description": "Lists Azure API Center services within an Azure subscription.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ApiCenter/services",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/apic#az-apic-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List services in resource group",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az apic show",
            "description": "Show details of an Azure API Center service instance.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ApiCenter/services/{serviceName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/apic#az-apic-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-group]",
                            "[--name]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Show service details",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.serviceName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az apic metadata list",
            "description": "List a collection of metadata schemas.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ApiCenter/services/{serviceName}/metadataSchemas",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/apic/metadata#az-apic-metadata-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-group]",
                            "[--service-name]",
                            "[--subscription]",
                            "[--filter]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List schemas",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--service-name",
                            "value": "[path.serviceName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}