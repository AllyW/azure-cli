{
    "resourceType": "routeFilters",
    "apiVersion": "2021-08-01",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/network/route-filter"
    },
    "commands": [
        {
            "name": "az network route-filter delete",
            "description": "Delete a route filter.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/routeFilters/{routeFilterName}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/network/route-filter#az-network-route-filter-delete"
                },
                "parameterSets": {
                    "parameters": [
                        "[--resource-group]",
                        "[--name]",
                        "[--subscription]"
                    ]
                }
            },
            "examples": [
                {
                    "description": "Delete a route filter.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.routeFilterName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az network route-filter list",
            "description": "List route filters.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/routeFilters",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/network/route-filter#az-network-route-filter-list"
                },
                "parameterSets": {
                    "parameters": [
                        "[--resource-group]",
                        "[--subscription]"
                    ]
                }
            },
            "examples": [
                {
                    "description": "List route filters in a resource group.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az network route-filter show",
            "description": "Get the details of a route filter.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/routeFilters/{routeFilterName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/network/route-filter#az-network-route-filter-show"
                },
                "parameterSets": {
                    "parameters": [
                        "[--resource-group]",
                        "[--name]",
                        "[--subscription]",
                        "[--expand]"
                    ]
                }
            },
            "examples": [
                {
                    "description": "Get the details of a route filter.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.routeFilterName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az network route-filter rule list",
            "description": "List rules in a route filter.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/routeFilters/{routeFilterName}/routeFilterRules",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/network/route-filter/rule#az-network-route-filter-rule-list"
                },
                "parameterSets": {
                    "parameters": [
                        "[--resource-group]",
                        "[--filter-name]",
                        "[--subscription]"
                    ]
                }
            },
            "examples": [
                {
                    "description": "List rules in a route filter.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--filter-name",
                            "value": "[path.routeFilterName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}