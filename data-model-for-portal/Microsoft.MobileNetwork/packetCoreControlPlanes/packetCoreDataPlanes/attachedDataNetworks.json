                        },
                        {
                            "name": "--pccp-name",
                            "value": "[path.packetCoreControlPlaneName]"
                        },
                        {
                            "name": "--pcdp-name",
                            "value": "[path.packetCoreDataPlaneName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
