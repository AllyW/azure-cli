{
    "resourceType": "backupVaults/backupPolicies",
    "apiVersion": "2024-04-01",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/dataprotection/backup-policy"
    },
    "commands": [
        {
            "name": "az dataprotection backup-policy delete",
            "description": "Deletes a backup policy belonging to a backup vault.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.DataProtection/backupVaults/{vaultName}/backupPolicies/{backupPolicyName}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/dataprotection/backup-policy#az-dataprotection-backup-policy-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--backup-policy-name]",
                            "[--resource-group]",
                            "[--subscription]",
                            "[--vault-name]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete Backup Policy",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.backupPolicyName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--vault-name",
                            "value": "[path.vaultName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az dataprotection backup-policy show",
            "description": "Get a backup policy belonging to a backup vault",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.DataProtection/backupVaults/{vaultName}/backupPolicies/{backupPolicyName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/dataprotection/backup-policy#az-dataprotection-backup-policy-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--backup-policy-name]",
                            "[--resource-group]",
                            "[--subscription]",
                            "[--vault-name]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Get Backup Policy",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.backupPolicyName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--vault-name",
                            "value": "[path.vaultName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}