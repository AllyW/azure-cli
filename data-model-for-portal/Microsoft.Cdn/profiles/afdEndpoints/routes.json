{
    "resourceType": "profiles/afdEndpoints/routes",
    "apiVersion": "2024-02-01",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/afd/route"
    },
    "commands": [
        {
            "name": "az afd route delete",
            "description": "Delete an existing route with the specified route name under the specified subscription, resource group, profile, and AzureFrontDoor endpoint.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Cdn/profiles/{profileName}/afdEndpoints/{endpointName}/routes/{routeName}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/afd/route#az-afd-route-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--endpoint-name]",
                            "[--profile-name]",
                            "[--resource-group]",
                            "[--name]",
                            "[--route-name]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete an route named route1.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--profile-name",
                            "value": "[path.profileName]"
                        },
                        {
                            "name": "--endpoint-name",
                            "value": "[path.endpointName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.routeName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az afd route show",
            "description": "Get an existing route with the specified route name under the specified subscription, resource group, profile, and AzureFrontDoor endpoint.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Cdn/profiles/{profileName}/afdEndpoints/{endpointName}/routes/{routeName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/afd/route#az-afd-route-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--endpoint-name]",
                            "[--profile-name]",
                            "[--resource-group]",
                            "[--name]",
                            "[--route-name]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Get an existing route with the specified route name under the specified subscription, resource group, profile, and AzureFrontDoor endpoint.",
                    "parameters": [
                        {
                            "name": "--endpoint-name",
                            "value": "[path.endpointName]"
                        },
                        {
                            "name": "--profile-name",
                            "value": "[path.profileName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.routeName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}