{
    "resourceType": "virtualMachines/consoles",
    "apiVersion": "2023-10-01-preview",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/networkcloud/virtualmachine/console"
    },
    "commands": [
        {
            "name": "az networkcloud virtualmachine console delete",
            "description": "Delete the provided virtual machine console.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.NetworkCloud/virtualMachines/{virtualMachineName}/consoles/{consoleName}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/networkcloud/virtualmachine/console#az-networkcloud-virtualmachine-console-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--console-name]",
                            "[--resource-group]",
                            "[--subscription]",
                            "[--virtual-machine-name]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete the provided virtual machine console.",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.consoleName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        },
                        {
                            "name": "--virtual-machine-name",
                            "value": "[path.virtualMachineName]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az networkcloud virtualmachine console show",
            "description": "Get properties of the provided virtual machine console.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.NetworkCloud/virtualMachines/{virtualMachineName}/consoles/{consoleName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/networkcloud/virtualmachine/console#az-networkcloud-virtualmachine-console-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--console-name]",
                            "[--resource-group]",
                            "[--subscription]",
                            "[--virtual-machine-name]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Get properties of the provided virtual machine console.",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.consoleName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        },
                        {
                            "name": "--virtual-machine-name",
                            "value": "[path.virtualMachineName]"
                        }
                    ]
                }
            ]
        }
    ]
}