{
    "resourceType": "netAppAccounts/backupVaults",
    "apiVersion": "2022-11-01-preview",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/netappfiles/account/backup-vault"
    },
    "commands": [
        {
            "name": "az netappfiles account backup-vault delete",
            "description": "Delete the specified Backup Vault",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.NetApp/netAppAccounts/{accountName}/backupVaults/{backupVaultName}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/netappfiles/account/backup-vault#az-netappfiles-account-backup-vault-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--account-name]",
                            "[--name]",
                            "[--backup-vault-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete the specified Backup Vault",
                    "parameters": [
                        {
                            "name": "--account-name",
                            "value": "[path.accountName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.backupVaultName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az netappfiles account backup-vault show",
            "description": "Get the Backup Vault",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.NetApp/netAppAccounts/{accountName}/backupVaults/{backupVaultName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/netappfiles/account/backup-vault#az-netappfiles-account-backup-vault-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--account-name]",
                            "[--name]",
                            "[--backup-vault-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Get the Backup Vault",
                    "parameters": [
                        {
                            "name": "--account-name",
                            "value": "[path.accountName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.backupVaultName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az netappfiles account backup-vault backup list",
            "description": "List all backups Under a Backup Vault",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.NetApp/netAppAccounts/{accountName}/backupVaults/{backupVaultName}/backups",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/netappfiles/account/backup-vault/backup#az-netappfiles-account-backup-vault-backup-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--account-name]",
                            "[--backup-vault-name]",
                            "[--resource-group]",
                            "[--subscription]",
                            "[--filter]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List all backups Under a Backup Vault",
                    "parameters": [
                        {
                            "name": "--account-name",
                            "value": "[path.accountName]"
                        },
                        {
                            "name": "--backup-vault-name",
                            "value": "[path.backupVaultName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}