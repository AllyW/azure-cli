                    }
                ]
            },
            "examples": [
                {
                    "description": "Return operation results.",
                    "parameters": [
                        {
                            "name": "--location",
                            "value": "[path.location]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.operationId]"
