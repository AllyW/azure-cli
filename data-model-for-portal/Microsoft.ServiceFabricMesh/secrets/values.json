{
    "resourceType": "secrets/values",
    "apiVersion": "2018-09-01-preview",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/mesh/secretvalue"
    },
    "commands": [
        {
            "name": "az mesh secretvalue delete",
            "description": "Delete a secret version.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ServiceFabricMesh/secrets/{secretResourceName}/values/{secretValueResourceName}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/mesh/secretvalue#az-mesh-secretvalue-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-group]",
                            "[--secret-resource-name]",
                            "[--version]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete a secret version.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--secret-resource-name",
                            "value": "[path.secretResourceName]"
                        },
                        {
                            "name": "--version",
                            "value": "[path.secretValueResourceName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}