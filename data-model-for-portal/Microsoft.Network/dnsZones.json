                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-group]",
                            "[--subscription]",
                            "[--zone-name]",
                            "[--recordsetnamesuffix]",
                            "[--top]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List all record sets within a DNS zone.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        },
                        {
                            "name": "--zone-name",
                            "value": "[path.zoneName]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az network dns zone delete",
