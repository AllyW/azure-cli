                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--authorization-rule-name]",
                            "[--namespace-name]",
                            "[--queue-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List the keys and connection strings of Authorization Rule for the given Service Bus Queue",
                    "parameters": [
                        {
