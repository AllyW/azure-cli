{
    "resourceType": "loadBalancers/probes",
    "apiVersion": "2022-05-01",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/network/lb/probe"
    },
    "commands": [
        {
            "name": "az network lb probe show",
            "description": "Get the details of a probe.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/loadBalancers/{loadBalancerName}/probes/{probeName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/network/lb/probe#az-network-lb-probe-show"
                },
                "parameterSets": {
                    "parameters": [
                        "[--lb-name]",
                        "[--name]",
                        "[--resource-group]",
                        "[--subscription]"
                    ]
                }
            },
            "examples": [
                {
                    "description": "Get the details of a probe.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--lb-name",
                            "value": "[path.loadBalancerName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.probeName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}