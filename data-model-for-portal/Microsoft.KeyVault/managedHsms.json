{
    "resourceType": "managedHsms",
    "apiVersion": "2021-04-01-preview",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/keyvault"
    },
    "commands": [
        {
            "name": "az keyvault show",
            "description": "Show details of a MHSM",
            "path":"/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.KeyVault/managedHSMs/{name}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/keyvault#az-keyvault-show"
                },
                "parameterSets":[
                    {
                        "parameters": [
                            "--hsm-name",
                            "--resource-group",
                            "--subscription"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Show details of a managed HSM in your resource group.",
                    "parameters`": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--hsm-name",
                            "value": "[path.name]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az keyvault delete",
            "description": "Delete a MHSM",
            "path":"/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.KeyVault/managedHSMs/{name}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/keyvault#az-keyvault-delete"
                },
                "parameterSets":[
                    {
                        "parameters": [
                            "--hsm-name",
                            "--resource-group",
                            "--subscription",
                            "[--no-wait]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete specific managed HSM.",
                    "parameters`": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--hsm-name",
                            "value": "[path.name]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az keyvault list",
            "description": "List MHSMs within specified resource group",
            "path":"/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.KeyVault/managedHSMs",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/keyvault#az-keyvault-list"
                },
                "parameterSets":[
                    {
                        "parameters": [
                            "--resource-group",
                            "--resource-type",
                            "--subscription"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List managed HSMs in your resource group.",
                    "parameters`": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--resource-type",
                            "value": "hsm"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az keyvault list-deleted",
            "description": "List deleted MHSMs in a subscription",
            "path":"/subscriptions/{subscriptionId}/providers/Microsoft.KeyVault/deletedManagedHSMs",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/keyvault#az-keyvault-list-deleted"
                },
                "parameterSets":[
                    {
                        "parameters": [
                            "--resource-type",
                            "--subscription"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List deleted vaults in a subscription.",
                    "parameters`": [
                        {
                            "name": "--resource-type",
                            "value": "hsm"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az keyvault show-deleted",
            "description": "Show details of a deleted MHSM",
            "path":"/subscriptions/{subscriptionId}/providers/Microsoft.KeyVault/locations/{location}/deletedManagedHSMs/{name}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/keyvault#az-keyvault-show-deleted"
                },
                "parameterSets":[
                    {
                        "parameters": [
                            "--hsm-name",
                            "--location",
                            "--subscription"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Show details of a deleted managed HSM.",
                    "parameters`": [
                        {
                            "name": "--location",
                            "value": "[path.location]"
                        },
                        {
                            "name": "--hsm-name",
                            "value": "[path.name]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az keyvault purge",
            "description": "Permanently delete a MHSM",
            "path":"/subscriptions/{subscriptionId}/providers/Microsoft.KeyVault/locations/{location}/deletedManagedHSMs/{name}/purge",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/keyvault#az-keyvault-purge"
                },
                "parameterSets":[
                    {
                        "parameters": [
                            "--hsm-name",
                            "--location",
                            "--subscription",
                            "[--no-wait]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Permanently delete the specified managed HSM.",
                    "parameters`": [
                        {
                            "name": "--location",
                            "value": "[path.location]"
                        },
                        {
                            "name": "--hsm-name",
                            "value": "[path.name]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az keyvault private-link-resource list",
            "description": "List the private link resources supported for a MHSM",
            "path":"/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.KeyVault/managedHSMs/{name}/privateLinkResources",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/keyvault/private-link-resource#az-keyvault-private-link-resource-list"
                },
                "parameterSets":[
                    {
                        "parameters": [
                            "--hsm-name",
                            "--resource-group",
                            "--subscription"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List the private link resources supported for the managed HSM.",
                    "parameters`": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--hsm-name",
                            "value": "[path.name]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az keyvault private-endpoint-connection list",
            "description": "List all private endpoint connections associated with a MHSM",
            "path":"/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.KeyVault/managedHSMs/{name}/privateEndpointConnections",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/keyvault/private-endpoint-connection#az-keyvault-private-endpoint-connection-list"
                },
                "parameterSets":[
                    {
                        "parameters": [
                            "--hsm-name",
                            "--resource-group",
                            "--subscription"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List all private endpoint connections associated with the specified managed HSM.",
                    "parameters`": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--hsm-name",
                            "value": "[path.name]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}