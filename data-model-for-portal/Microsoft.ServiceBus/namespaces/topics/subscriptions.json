    "commands": [
        {
            "name": "az servicebus topic subscription delete",
            "description": "Delete a subscription from the specified topic.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ServiceBus/namespaces/{namespaceName}/topics/{topicName}/subscriptions/{subscriptionName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/servicebus/topic/subscription#az-servicebus-topic-subscription-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--namespace-name]",
                            "[--resource-group]",
                            "[--subscription]",
                            "[--name]",
                            "[--subscription-name]",
                            "[--topic-name]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete a subscription from the specified topic.",
                    "parameters": [
                        {
                            "name": "--namespace-name",
                            "value": "[path.namespaceName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.subscriptionName]"
                        },
                        {
                            "name": "--topic-name",
                            "value": "[path.topicName]"
                        }
                    ]
                }
            ]
        },
