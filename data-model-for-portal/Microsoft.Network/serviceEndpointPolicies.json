{
    "resourceType": "serviceEndpointPolicies",
    "apiVersion": "2021-08-01",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/network/service-endpoint/policy"
    },
    "commands": [
        {
            "name": "az network service-endpoint policy delete",
            "description": "Delete a service endpoint policy.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/serviceEndpointPolicies/{serviceEndpointPolicyName}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/network/service-endpoint/policy#az-network-service-endpoint-policy-delete"
                },
                "parameterSets": {
                    "parameters": [
                        "[--resource-group]",
                        "[--name]",
                        "[--subscription]"
                    ]
                }
            },
            "examples": [
                {
                    "description": "Delete a service endpoint policy.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.serviceEndpointPolicyName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az network service-endpoint policy list",
            "description": "List service endpoint policies.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/serviceEndpointPolicies",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/network/service-endpoint/policy#az-network-service-endpoint-policy-list"
                },
                "parameterSets": {
                    "parameters": [
                        "[--resource-group]",
                        "[--subscription]"
                    ]
                }
            },
            "examples": [
                {
                    "description": "List service endpoint policies.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az network service-endpoint policy show",
            "description": "Get the details of a service endpoint policy.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/serviceEndpointPolicies/{serviceEndpointPolicyName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/network/service-endpoint/policy#az-network-service-endpoint-policy-show"
                },
                "parameterSets": {
                    "parameters": [
                        "[--resource-group]",
                        "[--name]",
                        "[--subscription]",
                        "[--expand]"
                    ]
                }
            },
            "examples": [
                {
                    "description": "Get the details of a service endpoint policy.",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.serviceEndpointPolicyName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az network service-endpoint policy-definition list",
            "description": "List service endpoint policy definitions.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/serviceEndpointPolicies/{serviceEndpointPolicyName}/serviceEndpointPolicyDefinitions",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/network/service-endpoint/policy-definition#az-network-service-endpoint-policy-definition-list"
                },
                "parameterSets": {
                    "parameters": [
                        "[--resource-group]",
                        "[--policy-name]",
                        "[--subscription]"
                    ]
                }
            },
            "examples": [
                {
                    "description": "List service endpoint policy definitions.",
                    "parameters": [
                        {
                            "name": "--policy-name",
                            "value": "[path.serviceEndpointPolicyName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}