{
    "resourceType": "volumes",
    "apiVersion": "2018-09-01-preview",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/mesh/volume"
    },
    "commands": [
        {
            "name": "az mesh volume delete",
            "description": "Delete a volume.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ServiceFabricMesh/volumes/{volumeResourceName}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/mesh/volume#az-mesh-volume-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-group]",
                            "[--subscription]",
                            "[--name]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete a volume.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.volumeResourceName]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az mesh volume list",
            "description": "List volumes.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ServiceFabricMesh/volumes",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/mesh/volume#az-mesh-volume-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List volumes.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az mesh volume show",
            "description": "Get the details of a volume.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ServiceFabricMesh/volumes/{volumeResourceName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/mesh/volume#az-mesh-volume-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-group]",
                            "[--subscription]",
                            "[--name]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Get the details of a volume.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.volumeResourceName]"
                        }
                    ]
                }
            ]
        }
    ]
}