    "commands": [
        {
            "name": "az k8s-runtime service delete",
            "description": "Delete a Service",
            "path": "/{resourceUri}/providers/Microsoft.KubernetesRuntime/services/{serviceName}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/k8s-runtime/service#az-k8s-runtime-service-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-uri]",
                            "[--service-name]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete storage class service",
                    "parameters": [
                        {
                            "name": "--resource-uri",
                            "value": "[path.resourceUri]"
                        },
                        {
                            "name": "--service-name",
                            "value": "[path.serviceName]"
                        }
                    ]
                }
            ]
        },
        {
