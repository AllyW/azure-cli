{
    "resourceType": "mobileNetworks/slices",
    "apiVersion": "2023-09-01",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/mobile-network/slice"
    },
    "commands": [
        {
            "name": "az mobile-network slice delete",
            "description": "Delete the specified network slice.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.MobileNetwork/mobileNetworks/{mobileNetworkName}/slices/{sliceName}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/mobile-network/slice#az-mobile-network-slice-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--mobile-network-name]",
                            "[--resource-group]",
                            "[--name]",
                            "[--slice-name]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete the specified network slice.",
                    "parameters": [
                        {
                            "name": "--mobile-network-name",
                            "value": "[path.mobileNetworkName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.sliceName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az mobile-network slice show",
            "description": "Get information about the specified network slice.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.MobileNetwork/mobileNetworks/{mobileNetworkName}/slices/{sliceName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/mobile-network/slice#az-mobile-network-slice-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--mobile-network-name]",
                            "[--resource-group]",
                            "[--name]",
                            "[--slice-name]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Show slice",
                    "parameters": [
                        {
                            "name": "--mobile-network-name",
                            "value": "[path.mobileNetworkName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.sliceName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}