{
    "resourceType": "locations",
    "apiVersion": "2023-08-01-preview",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/network/perimeter/onboarded-resources"
    },
    "commands": [
        {
            "name": "az network perimeter onboarded-resources list",
            "description": "Gets the list of resources that are onboarded with NSP. These resources can be associated with a network security perimeter.",
            "path": "/subscriptions/{subscriptionId}/providers/Microsoft.Network/locations/{location}/perimeterAssociableResourceTypes",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/network/perimeter/onboarded-resources#az-network-perimeter-onboarded-resources-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--location]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List NSP onboarded resources",
                    "parameters": [
                        {
                            "name": "--location",
                            "value": "[path.location]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}