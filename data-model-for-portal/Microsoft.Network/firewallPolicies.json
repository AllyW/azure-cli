{
    "resourceType": "firewallPolicies",
    "apiVersion": "2022-01-01",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/network/firewall/policy"
    },
    "commands": [
        {
            "name": "az network firewall policy delete",
            "description": "Delete an Azure firewall policy.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/firewallPolicies/{firewallPolicyName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/network/firewall/policy#az-network-firewall-policy-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete an Azure firewall policy.",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.firewallPolicyName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az network firewall policy list",
            "description": "List all Azure firewall policies.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/firewallPolicies",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/network/firewall/policy#az-network-firewall-policy-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List all Azure firewall policies.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az network firewall policy show",
            "description": "Show an Azure firewall policy.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/firewallPolicies/{firewallPolicyName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/network/firewall/policy#az-network-firewall-policy-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--resource-group]",
                            "[--subscription]",
                            "[--expand]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Show an Azure firewall policy.",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.firewallPolicyName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az network firewall policy rule-collection-group list",
            "description": "List all Azure firewall policy rule collection groups.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/firewallPolicies/{firewallPolicyName}/ruleCollectionGroups",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/network/firewall/policy/rule-collection-group#az-network-firewall-policy-rule-collection-group-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--policy-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List all Azure firewall policy rule collection groups.",
                    "parameters": [
                        {
                            "name": "--policy-name",
                            "value": "[path.firewallPolicyName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
