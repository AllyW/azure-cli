                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--authorization-rule-name]",
                            "[--eventhub-name]",
                            "[--namespace-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Deletes the Authorizationrule of Eventhub.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--namespace-name",
                            "value": "[path.namespaceName]"
                        },
                        {
                            "name": "--eventhub-name",
                            "value": "[path.eventHubName]"
