{
    "resourceType": "namespaces/eventhubs/consumergroups",
    "apiVersion": "2023-01-01-preview",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/eventhubs/eventhub/consumer-group"
    },
    "commands": [
        {
            "name": "az eventhubs eventhub consumer-group delete",
            "description": "Delete a consumer group from the specified Event Hub and resource group.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.EventHub/namespaces/{namespaceName}/eventhubs/{eventHubName}/consumergroups/{consumerGroupName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/eventhubs/eventhub/consumer-group#az-eventhubs-eventhub-consumer-group-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--consumer-group-name]",
                            "[--eventhub-name]",
                            "[--namespace-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete a consumer group from the specified Event Hub and resource group.",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.consumerGroupName]"
                        },
                        {
                            "name": "--eventhub-name",
                            "value": "[path.eventHubName]"
                        },
                        {
                            "name": "--namespace-name",
                            "value": "[path.namespaceName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az eventhubs eventhub consumer-group show",
            "description": "Get a description for the specified consumer group.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.EventHub/namespaces/{namespaceName}/eventhubs/{eventHubName}/consumergroups/{consumerGroupName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/eventhubs/eventhub/consumer-group#az-eventhubs-eventhub-consumer-group-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--consumer-group-name]",
                            "[--eventhub-name]",
                            "[--namespace-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Get a description for the specified consumer group.",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.consumerGroupName]"
                        },
                        {
                            "name": "--eventhub-name",
                            "value": "[path.eventHubName]"
                        },
                        {
                            "name": "--namespace-name",
                            "value": "[path.namespaceName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}