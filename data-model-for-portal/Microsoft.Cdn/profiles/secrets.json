{
    "resourceType": "profiles/secrets",
    "apiVersion": "2024-02-01",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/afd/secret"
    },
    "commands": [
        {
            "name": "az afd secret delete",
            "description": "Delete an existing Secret within profile.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Cdn/profiles/{profileName}/secrets/{secretName}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/afd/secret#az-afd-secret-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--profile-name]",
                            "[--resource-group]",
                            "[--name]",
                            "[--secret-name]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete an existing Secret within profile.",
                    "parameters": [
                        {
                            "name": "--profile-name",
                            "value": "[path.profileName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.secretName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az afd secret show",
            "description": "Get an existing Secret within a profile.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Cdn/profiles/{profileName}/secrets/{secretName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/afd/secret#az-afd-secret-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--profile-name]",
                            "[--resource-group]",
                            "[--name]",
                            "[--secret-name]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Get an existing Secret within a profile.",
                    "parameters": [
                        {
                            "name": "--profile-name",
                            "value": "[path.profileName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.secretName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}