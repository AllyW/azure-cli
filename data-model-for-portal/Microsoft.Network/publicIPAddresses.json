{
    "resourceType": "publicIPAddresses",
    "apiVersion": "2022-05-01",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/network/public-ip"
    },
    "commands": [
        {
            "name": "az network public-ip delete",
            "description": "Delete a public IP address.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/publicIPAddresses/{publicIpAddressName}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/network/public-ip#az-network-public-ip-delete"
                },
                "parameterSets": {
                    "parameters": [
                        "[--name]",
                        "[--resource-group]",
                        "[--subscription]"
                    ]
                }
            },
            "examples": [
                {
                    "description": "Delete a public IP address.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.publicIpAddressName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az network public-ip list",
            "description": "List public IP addresses.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/publicIPAddresses",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/network/public-ip#az-network-public-ip-list"
                },
                "parameterSets": {
                    "parameters": [
                        "[--resource-group]",
                        "[--subscription]"
                    ]
                }
            },
            "examples": [
                {
                    "description": "List all public IPs in a subscription.",
                    "parameters": [
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az network public-ip show",
            "description": "Get the details of a public IP address.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/publicIPAddresses/{publicIpAddressName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/network/public-ip#az-network-public-ip-show"
                },
                "parameterSets": {
                    "parameters": [
                        "[--name]",
                        "[--resource-group]",
                        "[--subscription]",
                        "[--expand]"
                    ]
                }
            },
            "examples": [
                {
                    "description": "Get information about a public IP resource.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.publicIpAddressName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}