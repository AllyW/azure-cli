                    {
                        "parameters": [
                            "[--resource-uri]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List all services for a cluster",
                    "parameters": [
                        {
                            "name": "--resource-uri",
                            "value": "[path.resourceUri]"
                        }
                    ]
                }
            ]
        },
