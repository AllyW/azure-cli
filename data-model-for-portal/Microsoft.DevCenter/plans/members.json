            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/devcenter/admin/plan-member#az-devcenter-admin-plan-member-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
