{
    "resourceType": "threatIntelligence/indicators",
    "apiVersion": "2022-06-01-preview",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/sentinel/threat-indicator"
    },
    "commands": [
        {
            "name": "az sentinel threat-indicator delete",
            "description": "Delete a threat intelligence indicator.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.OperationalInsights/workspaces/{workspaceName}/providers/Microsoft.SecurityInsights/threatIntelligence/main/indicators/{name}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/sentinel/threat-indicator#az-sentinel-threat-indicator-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--resource-group]",
                            "[--subscription]",
                            "[--workspace-name]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete a threat intelligence indicator.",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.name]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        },
                        {
                            "name": "--workspace-name",
                            "value": "[path.workspaceName]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az sentinel threat-indicator list",
            "description": "Get all threat intelligence indicators.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.OperationalInsights/workspaces/{workspaceName}/providers/Microsoft.SecurityInsights/threatIntelligence/main/indicators",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/sentinel/threat-indicator#az-sentinel-threat-indicator-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-group]",
                            "[--subscription]",
                            "[--workspace-name]",
                            "[--filter]",
                            "[--orderby]",
                            "[--skip-token]",
                            "[--top]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Get all threat intelligence indicators.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
