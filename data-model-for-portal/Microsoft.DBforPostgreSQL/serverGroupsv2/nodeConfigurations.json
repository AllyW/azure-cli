{
    "resourceType": "serverGroupsv2/nodeConfigurations",
    "apiVersion": "2022-11-08",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/cosmosdb/postgres/configuration/node"
    },
    "commands": [
        {
            "name": "az cosmosdb postgres configuration node show",
            "description": "Get information of a configuration for worker nodes.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.DBforPostgreSQL/serverGroupsv2/{clusterName}/nodeConfigurations/{configurationName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/cosmosdb/postgres/configuration/node#az-cosmosdb-postgres-configuration-node-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--cluster-name]",
                            "[--name]",
                            "[--configuration-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Get configuration details for node",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.configurationName]"
                        },
                        {
                            "name": "--cluster-name",
                            "value": "[path.clusterName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}