                    "url": "https://docs.microsoft.com/cli/azure/automation/runtime-environment#az-automation-runtime-environment-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--account]",
                            "[--automation-account-name]",
                            "[--resource-group]",
                            "[--name]",
                            "[--runtime-environment-name]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete the Runtime Environment.",
                    "parameters": [
                        {
                            "name": "--account",
                            "value": "[path.automationAccountName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--name",
