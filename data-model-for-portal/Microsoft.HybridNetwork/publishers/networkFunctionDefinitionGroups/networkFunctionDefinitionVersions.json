                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--group-name]",
                            "[--version-name]",
                            "[--publisher-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Get information about a network function definition version.",
                    "parameters": [
                        {
                            "name": "--group-name",
                            "value": "[path.networkFunctionDefinitionGroupName]"
                        },
                        {
                            "name": "--version-name",
                            "value": "[path.networkFunctionDefinitionVersionName]"
                        },
                        {
                            "name": "--publisher-name",
