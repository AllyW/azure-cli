{
    "resourceType": "virtualNetworks",
    "apiVersion": "2022-07-01",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/dns-resolver"
    },
    "commands": [
        {
            "name": "az dns-resolver list-by-virtual-network",
            "description": "Lists DNS resolver resource IDs linked to a virtual network.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/virtualNetworks/{virtualNetworkName}/listDnsResolvers",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/dns-resolver#az-dns-resolver-list-by-virtual-network"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-group]",
                            "[--subscription]",
                            "[--virtual-network-name]",
                            "[--top]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List DNS resolvers by virtual network",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--virtual-network-name",
                            "value": "[path.virtualNetworkName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az dns-resolver forwarding-ruleset list-by-virtual-network",
            "description": "Lists DNS forwarding ruleset resource IDs attached to a virtual network.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/virtualNetworks/{virtualNetworkName}/listDnsForwardingRulesets",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/dns-resolver/forwarding-ruleset#az-dns-resolver-forwarding-ruleset-list-by-virtual-network"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-group]",
                            "[--subscription]",
                            "[--virtual-network-name]",
                            "[--top]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Lists DNS forwarding ruleset resource IDs attached to a virtual network.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        },
                        {
                            "name": "--virtual-network-name",
                            "value": "[path.virtualNetworkName]"
                        }
                    ]
                }
            ]
        }
    ]
}