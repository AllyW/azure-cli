{
    "resourceType": "namespaces/notificationHubs",
    "apiVersion": "2017-04-01",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/notification-hub"
    },
    "commands": [
        {
            "name": "az notification-hub delete",
            "description": "Delete a notification hub associated with a namespace.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.NotificationHubs/namespaces/{namespaceName}/notificationHubs/{notificationHubName}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/notification-hub#az-notification-hub-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--namespace-name]",
                            "[--name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete a notification hub",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--namespace-name",
                            "value": "[path.namespaceName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.notificationHubName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az notification-hub show",
            "description": "Show the notification hub information.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.NotificationHubs/namespaces/{namespaceName}/notificationHubs/{notificationHubName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/notification-hub#az-notification-hub-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--namespace-name]",
                            "[--name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Show the notification hub information.",
                    "parameters": [
                        {
                            "name": "--namespace-name",
                            "value": "[path.namespaceName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.notificationHubName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
