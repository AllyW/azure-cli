{
    "resourceType": "clusters",
    "apiVersion": "2023-01-01-preview",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/eventhubs/cluster"
    },
    "commands": [
        {
            "name": "az eventhubs cluster delete",
            "description": "Delete an existing Event Hubs Cluster. This operation is idempotent.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.EventHub/clusters/{clusterName}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/eventhubs/cluster#az-eventhubs-cluster-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--cluster-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete an existing Event Hubs Cluster. This operation is idempotent.",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.clusterName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az eventhubs cluster list",
            "description": "List the available Event Hubs Clusters within an ARM resource group",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.EventHub/clusters",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/eventhubs/cluster#az-eventhubs-cluster-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List the available Event Hubs Clusters within an ARM resource group",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az eventhubs cluster show",
            "description": "Get the resource description of the specified Event Hubs Cluster.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.EventHub/clusters/{clusterName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/eventhubs/cluster#az-eventhubs-cluster-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--cluster-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Get the resource description of the specified Event Hubs Cluster.",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.clusterName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az eventhubs cluster namespace list",
            "description": "List all Event Hubs Namespace IDs in an Event Hubs Dedicated Cluster.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.EventHub/clusters/{clusterName}/namespaces",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/eventhubs/cluster/namespace#az-eventhubs-cluster-namespace-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--cluster-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List all Event Hubs Namespace IDs in an Event Hubs Dedicated Cluster.",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.clusterName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}