{
    "resourceType": "networkSecurityGroups/securityRules",
    "apiVersion": "2021-08-01",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/network/nsg/rule"
    },
    "commands": [
        {
            "name": "az network nsg rule delete",
            "description": "Delete a network security group rule.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/networkSecurityGroups/{networkSecurityGroupName}/securityRules/{securityRuleName}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/network/nsg/rule#az-network-nsg-rule-delete"
                },
                "parameterSets": {
                    "parameters": [
                        "[--nsg-name]",
                        "[--resource-group]",
                        "[--name]",
                        "[--subscription]"
                    ]
                }
            },
            "examples": [
                {
                    "description": "Delete a network security group rule.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--nsg-name",
                            "value": "[path.networkSecurityGroupName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.securityRuleName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}