                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--name]",
                            "[--activity-log-alert-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete an activity log alert.",
                    "parameters": [
                        {
