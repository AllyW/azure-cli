{
    "resourceType": "networkPacketBrokers",
    "apiVersion": "2024-02-15-preview",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/networkfabric/npb"
    },
    "commands": [
        {
            "name": "az networkfabric npb list",
            "description": "List all Network Packet Brokers in the provided resource group or subscription",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ManagedNetworkFabric/networkPacketBrokers",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/networkfabric/npb#az-networkfabric-npb-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List the Network Packet Broker for Resource group",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az networkfabric npb show",
            "description": "Show details of the provided Network Packet Broker resource",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ManagedNetworkFabric/networkPacketBrokers/{networkPacketBrokerName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/networkfabric/npb#az-networkfabric-npb-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Show the Network Packet Broker",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--resource-name",
                            "value": "[path.networkPacketBrokerName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
}