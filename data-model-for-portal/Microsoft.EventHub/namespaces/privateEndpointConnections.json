                    {
                        "parameters": [
                            "[--namespace-name]",
                            "[--name]",
                            "[--private-endpoint-connection-name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete a Private Endpoint Connection.",
                    "parameters": [
                        {
                            "name": "--namespace-name",
